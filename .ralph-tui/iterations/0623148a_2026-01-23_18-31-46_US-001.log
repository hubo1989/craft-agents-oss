{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_vrtx_018Zh78LxNF4goiD6YA5DAfE","type":"tool_result","content":"     1â†’/**\n     2â†’ * Safe Mode Configuration\n     3â†’ *\n     4â†’ * Allows customization of Safe Mode rules per workspace and per source.\n     5â†’ * Users can create permissions.json files to extend the default rules.\n     6â†’ *\n     7â†’ * File locations:\n     8â†’ * - Workspace: ~/.craft-agent/workspaces/{slug}/permissions.json\n     9â†’ * - Per-source: ~/.craft-agent/workspaces/{slug}/sources/{sourceSlug}/permissions.json\n    10â†’ *\n    11â†’ * Rules are additive - custom configs extend the defaults (more permissive).\n    12â†’ */\n    13â†’\n    14â†’import { existsSync, readFileSync, mkdirSync, writeFileSync } from 'fs';\n    15â†’import { join } from 'path';\n    16â†’import { debug } from '../utils/debug.ts';\n    17â†’import { CONFIG_DIR } from '../config/paths.ts';\n    18â†’import { getSourcePath } from '../sources/storage.ts';\n    19â†’import {\n    20â†’  SAFE_MODE_CONFIG,\n    21â†’  PermissionsConfigSchema,\n    22â†’  type ApiEndpointRule,\n    23â†’  type PermissionsConfigFile,\n    24â†’  type CompiledApiEndpointRule,\n    25â†’  type CompiledBashPattern,\n    26â†’  type PermissionPaths,\n    27â†’} from './mode-types.ts';\n    28â†’\n    29â†’// ============================================================\n    30â†’// App-level Permissions Directory\n    31â†’// ============================================================\n    32â†’\n    33â†’const APP_PERMISSIONS_DIR = join(CONFIG_DIR, 'permissions');\n    34â†’\n    35â†’/**\n    36â†’ * Get the app-level permissions directory.\n    37â†’ * Default permissions are stored at ~/.craft-agent/permissions/\n    38â†’ */\n    39â†’export function getAppPermissionsDir(): string {\n    40â†’  return APP_PERMISSIONS_DIR;\n    41â†’}\n    42â†’\n    43â†’/**\n    44â†’ * Ensure default permissions file exists.\n    45â†’ * Copies bundled default.json from the provided directory on first run.\n    46â†’ * Only copies if file doesn't exist (preserves user edits).\n    47â†’ * @param bundledPermissionsDir - Path to bundled permissions (e.g., Electron's resources/permissions)\n    48â†’ * @param language - Language code (e.g., 'en', 'zh-CN') for selecting the appropriate template\n    49â†’ */\n    50â†’export function ensureDefaultPermissions(bundledPermissionsDir?: string, language?: string): void {\n    51â†’  const permissionsDir = getAppPermissionsDir();\n    52â†’\n    53â†’  // Create permissions directory if it doesn't exist\n    54â†’  if (!existsSync(permissionsDir)) {\n    55â†’    mkdirSync(permissionsDir, { recursive: true });\n    56â†’  }\n    57â†’\n    58â†’  // If no bundled permissions directory provided, just ensure the directory exists\n    59â†’  if (!bundledPermissionsDir || !existsSync(bundledPermissionsDir)) {\n    60â†’    return;\n    61â†’  }\n    62â†’\n    63â†’  // Copy default.json if it doesn't exist in app permissions dir\n    64â†’  const destPath = join(permissionsDir, 'default.json');\n    65â†’  if (!existsSync(destPath)) {\n    66â†’    const templateFile = language ? `default.${language}.json` : 'default.en.json';\n    67â†’    const srcPath = join(bundledPermissionsDir, templateFile);\n    68â†’\n    69â†’    // Fallback to English if the language-specific template doesn't exist\n    70â†’    const finalSrcPath = existsSync(srcPath)\n    71â†’      ? srcPath\n    72â†’      : join(bundledPermissionsDir, 'default.en.json');\n    73â†’\n    74â†’    if (existsSync(finalSrcPath)) {\n    75â†’      try {\n    76â†’        const content = readFileSync(finalSrcPath, 'utf-8');\n    77â†’        writeFileSync(destPath, content, 'utf-8');\n    78â†’        debug('[Permissions] Copied bundled permissions template to', destPath);\n    79â†’      } catch (error) {\n    80â†’        debug('[Permissions] Error copying bundled permissions template:', error);\n    81â†’      }\n    82â†’    }\n    83â†’  }\n    84â†’}\n    85â†’\n    86â†’/**\n    87â†’ * Update default permissions file to match the current language.\n    88â†’ * Preserves user customizations while updating system default comments.\n    89â†’ * @param bundledPermissionsDir - Path to bundled permissions templates\n    90â†’ * @param language - Target language code (e.g., 'en', 'zh-CN')\n    91â†’ */\n    92â†’export function updateDefaultPermissionsLanguage(bundledPermissionsDir: string, language: string): void {\n    93â†’  const permissionsDir = getAppPermissionsDir();\n    94â†’  const destPath = join(permissionsDir, 'default.json');\n    95â†’\n    96â†’  if (!existsSync(bundledPermissionsDir)) {\n    97â†’    debug('[Permissions] Bundled permissions directory not found:', bundledPermissionsDir);\n    98â†’    return;\n    99â†’  }\n   100â†’\n   101â†’  // Load the new language template\n   102â†’  const templateFile = `default.${language}.json`;\n   103â†’  const templatePath = join(bundledPermissionsDir, templateFile);\n   104â†’\n   105â†’  // Fallback to English if the language-specific template doesn't exist\n   106â†’  const finalTemplatePath = existsSync(templatePath)\n   107â†’    ? templatePath\n   108â†’    : join(bundledPermissionsDir, 'default.en.json');\n   109â†’\n   110â†’  if (!existsSync(finalTemplatePath)) {\n   111â†’    debug('[Permissions] Template not found:', finalTemplatePath);\n   112â†’    return;\n   113â†’  }\n   114â†’\n   115â†’  try {\n   116â†’    // Load existing user config (if any)\n   117â†’    let userConfig: PermissionsConfigFile | null = null;\n   118â†’    if (existsSync(destPath)) {\n   119â†’      const existingContent = readFileSync(destPath, 'utf-8');\n   120â†’      userConfig = parsePermissionsJson(existingContent);\n   121â†’    }\n   122â†’\n   123â†’    // Load the new template\n   124â†’    const templateContent = readFileSync(finalTemplatePath, 'utf-8');\n   125â†’    const templateConfig = JSON.parse(templateContent);\n   126â†’\n   127â†’    // Merge user customizations with new template\n   128â†’    const mergedConfig = mergePermissionsConfigs(templateConfig, userConfig);\n   129â†’\n   130â†’    // Write the merged config\n   131â†’    writeFileSync(destPath, JSON.stringify(mergedConfig, null, 2), 'utf-8');\n   132â†’    debug('[Permissions] Updated default permissions to language:', language);\n   133â†’  } catch (error) {\n   134â†’    debug('[Permissions] Error updating permissions language:', error);\n   135â†’  }\n   136â†’}\n   137â†’\n   138â†’/**\n   139â†’ * Merge user customizations into a new template.\n   140â†’ * Preserves user-added patterns while updating system default comments.\n   141â†’ */\n   142â†’function mergePermissionsConfigs(\n   143â†’  template: any,\n   144â†’  userConfig: PermissionsConfigFile | null\n   145â†’): any {\n   146â†’  if (!userConfig) {\n   147â†’    return template;\n   148â†’  }\n   149â†’\n   150â†’  // Start with the template as base\n   151â†’  const merged = JSON.parse(JSON.stringify(template));\n   152â†’\n   153â†’  // Helper to check if a pattern exists in template\n   154â†’  const patternExistsInTemplate = (pattern: string, templateArray: any[]): boolean => {\n   155â†’    return templateArray.some(item => {\n   156â†’      const itemPattern = typeof item === 'string' ? item : item.pattern;\n   157â†’      return itemPattern === pattern;\n   158â†’    });\n   159â†’  };\n   160â†’\n   161â†’  // Merge bash patterns - add user custom patterns not in template\n   162â†’  if (userConfig.allowedBashPatterns) {\n   163â†’    const templateBashPatterns = template.allowedBashPatterns || [];\n   164â†’    userConfig.allowedBashPatterns.forEach(userItem => {\n   165â†’      const userPattern = typeof userItem === 'string' ? userItem : userItem.pattern;\n   166â†’      if (!patternExistsInTemplate(userPattern, templateBashPatterns)) {\n   167â†’        // User added this custom pattern - preserve it\n   168â†’        merged.allowedBashPatterns = merged.allowedBashPatterns || [];\n   169â†’        merged.allowedBashPatterns.push(userItem);\n   170â†’      }\n   171â†’    });\n   172â†’  }\n   173â†’\n   174â†’  // Merge MCP patterns - add user custom patterns not in template\n   175â†’  if (userConfig.allowedMcpPatterns) {\n   176â†’    const templateMcpPatterns = template.allowedMcpPatterns || [];\n   177â†’    userConfig.allowedMcpPatterns.forEach(userItem => {\n   178â†’      const userPattern = typeof userItem === 'string' ? userItem : userItem.pattern;\n   179â†’      if (!patternExistsInTemplate(userPattern, templateMcpPatterns)) {\n   180â†’        merged.allowedMcpPatterns = merged.allowedMcpPatterns || [];\n   181â†’        merged.allowedMcpPatterns.push(userItem);\n   182â†’      }\n   183â†’    });\n   184â†’  }\n   185â†’\n   186â†’  // Merge API endpoints - preserve user additions\n   187â†’  if (userConfig.allowedApiEndpoints && userConfig.allowedApiEndpoints.length > 0) {\n   188â†’    merged.allowedApiEndpoints = [\n   189â†’      ...(template.allowedApiEndpoints || []),\n   190â†’      ...userConfig.allowedApiEndpoints\n   191â†’    ];\n   192â†’  }\n   193â†’\n   194â†’  // Merge write paths - preserve user additions\n   195â†’  if (userConfig.allowedWritePaths && userConfig.allowedWritePaths.length > 0) {\n   196â†’    const templateWritePaths = template.allowedWritePaths || [];\n   197â†’    userConfig.allowedWritePaths.forEach(userItem => {\n   198â†’      const userPath = typeof userItem === 'string' ? userItem : userItem.pattern;\n   199â†’      if (!patternExistsInTemplate(userPath, templateWritePaths)) {\n   200â†’        merged.allowedWritePaths = merged.allowedWritePaths || [];\n   201â†’        merged.allowedWritePaths.push(userItem);\n   202â†’      }\n   203â†’    });\n   204â†’  }\n   205â†’\n   206â†’  return merged;\n   207â†’}\n   208â†’\n   209â†’/**\n   210â†’ * Load default permissions from ~/.craft-agent/permissions/default.json\n   211â†’ * Returns null if file doesn't exist or is invalid.\n   212â†’ */\n   213â†’export function loadDefaultPermissions(): PermissionsCustomConfig | null {\n   214â†’  const defaultPath = join(getAppPermissionsDir(), 'default.json');\n   215â†’  if (!existsSync(defaultPath)) {\n   216â†’    debug('[Permissions] No default.json found at', defaultPath);\n   217â†’    return null;\n   218â†’  }\n   219â†’\n   220â†’  try {\n   221â†’    const content = readFileSync(defaultPath, 'utf-8');\n   222â†’    const config = parsePermissionsJson(content);\n   223â†’    debug('[Permissions] Loaded default permissions from', defaultPath);\n   224â†’    return config;\n   225â†’  } catch (error) {\n   226â†’    debug('[Permissions] Error loading default permissions:', error);\n   227â†’    return null;\n   228â†’  }\n   229â†’}\n   230â†’\n   231â†’// Re-export types from mode-types for external consumers\n   232â†’export {\n   233â†’  PermissionsConfigSchema,\n   234â†’  type ApiEndpointRule,\n   235â†’  type PermissionsConfigFile,\n   236â†’  type CompiledApiEndpointRule,\n   237â†’  type CompiledBashPattern,\n   238â†’  type PermissionPaths,\n   239â†’};\n   240â†’\n   241â†’// ============================================================\n   242â†’// Types\n   243â†’// ============================================================\n   244â†’\n   245â†’/**\n   246â†’ * Pattern entry with optional comment for error messages.\n   247â†’ * Preserves the comment from permissions.json so we can show helpful hints.\n   248â†’ */\n   249â†’export interface PatternWithComment {\n   250â†’  pattern: string;\n   251â†’  comment?: string;\n   252â†’}\n   253â†’\n   254â†’/**\n   255â†’ * Parsed and normalized permissions configuration\n   256â†’ *\n   257â†’ * Note: blockedTools (Write, Edit, MultiEdit, NotebookEdit) are hardcoded in\n   258â†’ * SAFE_MODE_CONFIG and not configurable here - they're fundamental write\n   259â†’ * operations that must always be blocked in Explore mode.\n   260â†’ */\n   261â†’export interface PermissionsCustomConfig {\n   262â†’  /** Additional bash patterns to allow (with optional comments for error messages) */\n   263â†’  allowedBashPatterns: PatternWithComment[];\n   264â†’  /** Additional MCP patterns to allow (as regex strings) */\n   265â†’  allowedMcpPatterns: string[];\n   266â†’  /** API endpoint rules for fine-grained control */\n   267â†’  allowedApiEndpoints: ApiEndpointRule[];\n   268â†’  /** File paths to allow writes in Explore mode (glob pattern strings) */\n   269â†’  allowedWritePaths: string[];\n   270â†’}\n   271â†’\n   272â†’/**\n   273â†’ * Merged permissions config for runtime use\n   274â†’ */\n   275â†’export interface MergedPermissionsConfig {\n   276â†’  /** Blocked tools (Write, Edit, MultiEdit, NotebookEdit) - hardcoded, not configurable */\n   277â†’  blockedTools: Set<string>;\n   278â†’  /** Read-only bash patterns with metadata for helpful error messages */\n   279â†’  readOnlyBashPatterns: CompiledBashPattern[];\n   280â†’  readOnlyMcpPatterns: RegExp[];\n   281â†’  /** Fine-grained API endpoint rules */\n   282â†’  allowedApiEndpoints: CompiledApiEndpointRule[];\n   283â†’  /** File paths allowed for writes in Explore mode (glob patterns) */\n   284â†’  allowedWritePaths: string[];\n   285â†’  /** Display name for error messages */\n   286â†’  displayName: string;\n   287â†’  /** Keyboard shortcut hint */\n   288â†’  shortcutHint: string;\n   289â†’  /** Paths to permission files for actionable error messages */\n   290â†’  permissionPaths?: PermissionPaths;\n   291â†’}\n   292â†’\n   293â†’/**\n   294â†’ * Context for permissions checking (includes workspace/source/agent info)\n   295â†’ */\n   296â†’export interface PermissionsContext {\n   297â†’  workspaceRootPath: string;\n   298â†’  /** Active source slugs for source-specific rules */\n   299â†’  activeSourceSlugs?: string[];\n   300â†’}\n   301â†’\n   302â†’// ============================================================\n   303â†’// JSON Parser\n   304â†’// ============================================================\n   305â†’\n   306â†’/**\n   307â†’ * Parse and validate permissions.json file\n   308â†’ */\n   309â†’export function parsePermissionsJson(content: string): PermissionsCustomConfig {\n   310â†’  const emptyConfig: PermissionsCustomConfig = {\n   311â†’    allowedBashPatterns: [],\n   312â†’    allowedMcpPatterns: [],\n   313â†’    allowedApiEndpoints: [],\n   314â†’    allowedWritePaths: [],\n   315â†’  };\n   316â†’\n   317â†’  try {\n   318â†’    const json = JSON.parse(content);\n   319â†’    const result = PermissionsConfigSchema.safeParse(json);\n   320â†’\n   321â†’    if (!result.success) {\n   322â†’      debug('[SafeMode] Validation errors:', result.error.issues);\n   323â†’      // Log specific errors for debugging\n   324â†’      for (const issue of result.error.issues) {\n   325â†’        debug(`[SafeMode]   - ${issue.path.join('.')}: ${issue.message}`);\n   326â†’      }\n   327â†’      return emptyConfig;\n   328â†’    }\n   329â†’\n   330â†’    const data = result.data;\n   331â†’\n   332â†’    // Normalize patterns (extract string from pattern objects, but NOT for bash - preserve comments)\n   333â†’    const normalizePatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): string[] => {\n   334â†’      if (!patterns) return [];\n   335â†’      return patterns.map(p => typeof p === 'string' ? p : p.pattern);\n   336â†’    };\n   337â†’\n   338â†’    // For bash patterns, preserve comments for helpful error messages\n   339â†’    const normalizeBashPatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): PatternWithComment[] => {\n   340â†’      if (!patterns) return [];\n   341â†’      return patterns.map(p => {\n   342â†’        if (typeof p === 'string') {\n   343â†’          return { pattern: p };\n   344â†’        }\n   345â†’        return { pattern: p.pattern, comment: p.comment };\n   346â†’      });\n   347â†’    };\n   348â†’\n   349â†’    return {\n   350â†’      allowedBashPatterns: normalizeBashPatterns(data.allowedBashPatterns),\n   351â†’      allowedMcpPatterns: normalizePatterns(data.allowedMcpPatterns),\n   352â†’      allowedApiEndpoints: data.allowedApiEndpoints ?? [],\n   353â†’      allowedWritePaths: normalizePatterns(data.allowedWritePaths),\n   354â†’    };\n   355â†’  } catch (error) {\n   356â†’    debug('[SafeMode] JSON parse error:', error);\n   357â†’    return emptyConfig;\n   358â†’  }\n   359â†’}\n   360â†’\n   361â†’/**\n   362â†’ * Validate a regex pattern string, return null if invalid\n   363â†’ */\n   364â†’function validateRegex(pattern: string): RegExp | null {\n   365â†’  try {\n   366â†’    return new RegExp(pattern);\n   367â†’  } catch {\n   368â†’    return null;\n   369â†’  }\n   370â†’}\n   371â†’\n   372â†’/**\n   373â†’ * Validate permissions config and return errors\n   374â†’ */\n   375â†’export function validatePermissionsConfig(config: PermissionsConfigFile): string[] {\n   376â†’  const errors: string[] = [];\n   377â†’\n   378â†’  // Validate regex patterns\n   379â†’  const checkPatterns = (patterns: Array<string | { pattern: string }> | undefined, name: string) => {\n   380â†’    if (!patterns) return;\n   381â†’    for (let i = 0; i < patterns.length; i++) {\n   382â†’      const p = patterns[i];\n   383â†’      if (!p) continue;\n   384â†’      const patternStr = typeof p === 'string' ? p : p.pattern;\n   385â†’      if (!validateRegex(patternStr)) {\n   386â†’        errors.push(`${name}[${i}]: Invalid regex pattern: ${patternStr}`);\n   387â†’      }\n   388â†’    }\n   389â†’  };\n   390â†’\n   391â†’  checkPatterns(config.allowedBashPatterns, 'allowedBashPatterns');\n   392â†’  checkPatterns(config.allowedMcpPatterns, 'allowedMcpPatterns');\n   393â†’\n   394â†’  // Validate API endpoint patterns\n   395â†’  if (config.allowedApiEndpoints) {\n   396â†’    for (let i = 0; i < config.allowedApiEndpoints.length; i++) {\n   397â†’      const rule = config.allowedApiEndpoints[i];\n   398â†’      if (rule && !validateRegex(rule.path)) {\n   399â†’        errors.push(`allowedApiEndpoints[${i}].path: Invalid regex pattern: ${rule.path}`);\n   400â†’      }\n   401â†’    }\n   402â†’  }\n   403â†’\n   404â†’  return errors;\n   405â†’}\n   406â†’\n   407â†’// ============================================================\n   408â†’// Storage Functions\n   409â†’// ============================================================\n   410â†’\n   411â†’/**\n   412â†’ * Get path to workspace permissions.json\n   413â†’ */\n   414â†’export function getWorkspacePermissionsPath(workspaceRootPath: string): string {\n   415â†’  return join(workspaceRootPath, 'permissions.json');\n   416â†’}\n   417â†’\n   418â†’/**\n   419â†’ * Get path to source permissions.json\n   420â†’ */\n   421â†’export function getSourcePermissionsPath(workspaceRootPath: string, sourceSlug: string): string {\n   422â†’  return join(getSourcePath(workspaceRootPath, sourceSlug), 'permissions.json');\n   423â†’}\n   424â†’\n   425â†’/**\n   426â†’ * Load workspace-level permissions config\n   427â†’ */\n   428â†’export function loadWorkspacePermissionsConfig(workspaceRootPath: string): PermissionsCustomConfig | null {\n   429â†’  const path = getWorkspacePermissionsPath(workspaceRootPath);\n   430â†’  if (!existsSync(path)) return null;\n   431â†’\n   432â†’  try {\n   433â†’    const content = readFileSync(path, 'utf-8');\n   434â†’    const config = parsePermissionsJson(content);\n   435â†’    debug(`[Permissions] Loaded workspace config from ${path}:`, config);\n   436â†’    return config;\n   437â†’  } catch (error) {\n   438â†’    debug(`[Permissions] Error loading workspace config:`, error);\n   439â†’    return null;\n   440â†’  }\n   441â†’}\n   442â†’\n   443â†’/**\n   444â†’ * Load source-level permissions config\n   445â†’ */\n   446â†’export function loadSourcePermissionsConfig(\n   447â†’  workspaceRootPath: string,\n   448â†’  sourceSlug: string\n   449â†’): PermissionsCustomConfig | null {\n   450â†’  const path = getSourcePermissionsPath(workspaceRootPath, sourceSlug);\n   451â†’  if (!existsSync(path)) return null;\n   452â†’\n   453â†’  try {\n   454â†’    const content = readFileSync(path, 'utf-8');\n   455â†’    const config = parsePermissionsJson(content);\n   456â†’    debug(`[Permissions] Loaded source config from ${path}:`, config);\n   457â†’    return config;\n   458â†’  } catch (error) {\n   459â†’    debug(`[Permissions] Error loading source config:`, error);\n   460â†’    return null;\n   461â†’  }\n   462â†’}\n   463â†’\n   464â†’// ============================================================\n   465â†’// API Endpoint Checking\n   466â†’// ============================================================\n   467â†’\n   468â†’/**\n   469â†’ * Check if an API call is allowed by endpoint rules\n   470â†’ */\n   471â†’export function isApiEndpointAllowed(\n   472â†’  method: string,\n   473â†’  path: string,\n   474â†’  config: MergedPermissionsConfig\n   475â†’): boolean {\n   476â†’  const upperMethod = method.toUpperCase();\n   477â†’\n   478â†’  // GET is always allowed\n   479â†’  if (upperMethod === 'GET') return true;\n   480â†’\n   481â†’  // Check fine-grained endpoint rules\n   482â†’  for (const rule of config.allowedApiEndpoints) {\n   483â†’    if (rule.method === upperMethod && rule.pathPattern.test(path)) {\n   484â†’      return true;\n   485â†’    }\n   486â†’  }\n   487â†’\n   488â†’  return false;\n   489â†’}\n   490â†’\n   491â†’// ============================================================\n   492â†’// Config Cache\n   493â†’// ============================================================\n   494â†’\n   495â†’/**\n   496â†’ * In-memory cache for parsed permissions configs\n   497â†’ * Invalidated on file changes via ConfigWatcher\n   498â†’ */\n   499â†’class PermissionsConfigCache {\n   500â†’  private workspaceConfigs: Map<string, PermissionsCustomConfig | null> = new Map();\n   501â†’  private sourceConfigs: Map<string, PermissionsCustomConfig | null> = new Map();\n   502â†’  private mergedConfigs: Map<string, MergedPermissionsConfig> = new Map();\n   503â†’\n   504â†’  // App-level default permissions (loaded from ~/.craft-agent/permissions/default.json)\n   505â†’  private defaultConfig: PermissionsCustomConfig | null | undefined = undefined; // undefined = not loaded yet\n   506â†’\n   507â†’  /**\n   508â†’   * Get or load app-level default permissions\n   509â†’   * These come from ~/.craft-agent/permissions/default.json\n   510â†’   */\n   511â†’  private getDefaultConfig(): PermissionsCustomConfig | null {\n   512â†’    if (this.defaultConfig === undefined) {\n   513â†’      this.defaultConfig = loadDefaultPermissions();\n   514â†’    }\n   515â†’    return this.defaultConfig;\n   516â†’  }\n   517â†’\n   518â†’  /**\n   519â†’   * Get or load workspace config\n   520â†’   */\n   521â†’  getWorkspaceConfig(workspaceRootPath: string): PermissionsCustomConfig | null {\n   522â†’    if (!this.workspaceConfigs.has(workspaceRootPath)) {\n   523â†’      this.workspaceConfigs.set(workspaceRootPath, loadWorkspacePermissionsConfig(workspaceRootPath));\n   524â†’    }\n   525â†’    return this.workspaceConfigs.get(workspaceRootPath) ?? null;\n   526â†’  }\n   527â†’\n   528â†’  /**\n   529â†’   * Get or load source config\n   530â†’   */\n   531â†’  getSourceConfig(workspaceRootPath: string, sourceSlug: string): PermissionsCustomConfig | null {\n   532â†’    const key = `${workspaceRootPath}::${sourceSlug}`;\n   533â†’    if (!this.sourceConfigs.has(key)) {\n   534â†’      this.sourceConfigs.set(key, loadSourcePermissionsConfig(workspaceRootPath, sourceSlug));\n   535â†’    }\n   536â†’    return this.sourceConfigs.get(key) ?? null;\n   537â†’  }\n   538â†’\n   539â†’  /**\n   540â†’   * Invalidate app-level default permissions (called by ConfigWatcher)\n   541â†’   * This clears all merged configs since defaults affect everything\n   542â†’   */\n   543â†’  invalidateDefaults(): void {\n   544â†’    debug('[Permissions] Invalidating app-level default permissions');\n   545â†’    this.defaultConfig = undefined;\n   546â†’    // Clear ALL merged configs since defaults affect everything\n   547â†’    this.mergedConfigs.clear();\n   548â†’  }\n   549â†’\n   550â†’  /**\n   551â†’   * Invalidate workspace config (called by ConfigWatcher)\n   552â†’   */\n   553â†’  invalidateWorkspace(workspaceRootPath: string): void {\n   554â†’    debug(`[Permissions] Invalidating workspace config: ${workspaceRootPath}`);\n   555â†’    this.workspaceConfigs.delete(workspaceRootPath);\n   556â†’    // Clear all merged configs for this workspace\n   557â†’    for (const key of this.mergedConfigs.keys()) {\n   558â†’      if (key.startsWith(`${workspaceRootPath}::`)) {\n   559â†’        this.mergedConfigs.delete(key);\n   560â†’      }\n   561â†’    }\n   562â†’  }\n   563â†’\n   564â†’  /**\n   565â†’   * Invalidate source config (called by ConfigWatcher)\n   566â†’   */\n   567â†’  invalidateSource(workspaceRootPath: string, sourceSlug: string): void {\n   568â†’    debug(`[Permissions] Invalidating source config: ${workspaceRootPath}/${sourceSlug}`);\n   569â†’    this.sourceConfigs.delete(`${workspaceRootPath}::${sourceSlug}`);\n   570â†’    // Clear merged configs that include this source\n   571â†’    // Cache key format: \"{workspaceRootPath}::{source1},{source2},...\"\n   572â†’    // Use precise matching to avoid false positives (e.g., \"linear\" matching \"linear-triage\")\n   573â†’    for (const key of this.mergedConfigs.keys()) {\n   574â†’      if (!key.startsWith(`${workspaceRootPath}::`)) continue;\n   575â†’\n   576â†’      // Extract sources portion after the ::\n   577â†’      const sourcesStr = key.slice(workspaceRootPath.length + 2);\n   578â†’      if (!sourcesStr) continue;\n   579â†’\n   580â†’      // Check for exact match: at start, end, or between commas\n   581â†’      const sources = sourcesStr.split(',');\n   582â†’      if (sources.includes(sourceSlug)) {\n   583â†’        this.mergedConfigs.delete(key);\n   584â†’      }\n   585â†’    }\n   586â†’  }\n   587â†’\n   588â†’\n   589â†’  /**\n   590â†’   * Get merged config for a context (workspace + active sources)\n   591â†’   * Uses additive merging: custom configs extend defaults\n   592â†’   */\n   593â†’  getMergedConfig(context: PermissionsContext): MergedPermissionsConfig {\n   594â†’    const cacheKey = this.buildCacheKey(context);\n   595â†’\n   596â†’    if (!this.mergedConfigs.has(cacheKey)) {\n   597â†’      const merged = this.buildMergedConfig(context);\n   598â†’      this.mergedConfigs.set(cacheKey, merged);\n   599â†’    }\n   600â†’\n   601â†’    return this.mergedConfigs.get(cacheKey)!;\n   602â†’  }\n   603â†’\n   604â†’  private buildMergedConfig(context: PermissionsContext): MergedPermissionsConfig {\n   605â†’    const defaults = SAFE_MODE_CONFIG;\n   606â†’\n   607â†’    // Start with hardcoded fallback defaults (blocked tools are fixed, display settings)\n   608â†’    // blockedTools (Write, Edit, MultiEdit, NotebookEdit) come from SAFE_MODE_CONFIG\n   609â†’    // and cannot be modified via permissions.json\n   610â†’    const merged: MergedPermissionsConfig = {\n   611â†’      blockedTools: new Set(defaults.blockedTools),\n   612â†’      readOnlyBashPatterns: [...defaults.readOnlyBashPatterns],\n   613â†’      readOnlyMcpPatterns: [...defaults.readOnlyMcpPatterns],\n   614â†’      allowedApiEndpoints: [],\n   615â†’      allowedWritePaths: [],\n   616â†’      displayName: defaults.displayName,\n   617â†’      shortcutHint: defaults.shortcutHint,\n   618â†’      // Add permission file paths for actionable error messages\n   619â†’      permissionPaths: {\n   620â†’        workspacePath: getWorkspacePermissionsPath(context.workspaceRootPath),\n   621â†’        appDefaultPath: join(getAppPermissionsDir(), 'default.json'),\n   622â†’        docsPath: join(CONFIG_DIR, 'docs', 'permissions.md'),\n   623â†’      },\n   624â†’    };\n   625â†’\n   626â†’    // Load and apply app-level default permissions from JSON\n   627â†’    // This is where the actual bash/MCP patterns come from\n   628â†’    const defaultConfig = this.getDefaultConfig();\n   629â†’    if (defaultConfig) {\n   630â†’      this.applyDefaultConfig(merged, defaultConfig);\n   631â†’    }\n   632â†’\n   633â†’    // Add workspace-level customizations\n   634â†’    const wsConfig = this.getWorkspaceConfig(context.workspaceRootPath);\n   635â†’    if (wsConfig) {\n   636â†’      this.applyCustomConfig(merged, wsConfig);\n   637â†’    }\n   638â†’\n   639â†’    // Add source-level customizations (additive, with auto-scoped MCP patterns)\n   640â†’    if (context.activeSourceSlugs) {\n   641â†’      for (const sourceSlug of context.activeSourceSlugs) {\n   642â†’        const srcConfig = this.getSourceConfig(context.workspaceRootPath, sourceSlug);\n   643â†’        if (srcConfig) {\n   644â†’          // Use applySourceConfig which auto-scopes MCP patterns to this source\n   645â†’          this.applySourceConfig(merged, srcConfig, sourceSlug);\n   646â†’        }\n   647â†’      }\n   648â†’    }\n   649â†’\n   650â†’    return merged;\n   651â†’  }\n   652â†’\n   653â†’  /**\n   654â†’   * Apply app-level default config (from default.json)\n   655â†’   * This adds bash/MCP patterns from the JSON config. Blocked tools are hardcoded\n   656â†’   * in SAFE_MODE_CONFIG and not loaded from JSON.\n   657â†’   */\n   658â†’  private applyDefaultConfig(merged: MergedPermissionsConfig, config: PermissionsCustomConfig): void {\n   659â†’    // Add allowed bash patterns (as CompiledBashPattern with metadata for error messages)\n   660â†’    for (const patternEntry of config.allowedBashPatterns) {\n   661â†’      const regex = validateRegex(patternEntry.pattern);\n   662â†’      if (regex) {\n   663â†’        merged.readOnlyBashPatterns.push({\n   664â†’          regex,\n   665â†’          source: patternEntry.pattern,\n   666â†’          comment: patternEntry.comment,\n   667â†’        });\n   668â†’      } else {\n   669â†’        debug(`[Permissions] Invalid default bash pattern, skipping: ${patternEntry.pattern}`);\n   670â†’      }\n   671â†’    }\n   672â†’\n   673â†’    // Add allowed MCP patterns\n   674â†’    for (const pattern of config.allowedMcpPatterns) {\n   675â†’      const regex = validateRegex(pattern);\n   676â†’      if (regex) {\n   677â†’        merged.readOnlyMcpPatterns.push(regex);\n   678â†’      } else {\n   679â†’        debug(`[Permissions] Invalid default MCP pattern, skipping: ${pattern}`);\n   680â†’      }\n   681â†’    }\n   682â†’\n   683â†’    // Add allowed API endpoints\n   684â†’    for (const rule of config.allowedApiEndpoints) {\n   685â†’      const pathRegex = validateRegex(rule.path);\n   686â†’      if (pathRegex) {\n   687â†’        merged.allowedApiEndpoints.push({\n   688â†’          method: rule.method,\n   689â†’          pathPattern: pathRegex,\n   690â†’        });\n   691â†’      }\n   692â†’    }\n   693â†’\n   694â†’    // Add allowed write paths\n   695â†’    for (const pattern of config.allowedWritePaths) {\n   696â†’      merged.allowedWritePaths.push(pattern);\n   697â†’    }\n   698â†’  }\n   699â†’\n   700â†’  private applyCustomConfig(merged: MergedPermissionsConfig, custom: PermissionsCustomConfig): void {\n   701â†’    // Add allowed bash patterns (making config more permissive)\n   702â†’    for (const patternEntry of custom.allowedBashPatterns) {\n   703â†’      const regex = validateRegex(patternEntry.pattern);\n   704â†’      if (regex) {\n   705â†’        merged.readOnlyBashPatterns.push({\n   706â†’          regex,\n   707â†’          source: patternEntry.pattern,\n   708â†’          comment: patternEntry.comment,\n   709â†’        });\n   710â†’      } else {\n   711â†’        debug(`[Permissions] Invalid bash pattern, skipping: ${patternEntry.pattern}`);\n   712â†’      }\n   713â†’    }\n   714â†’\n   715â†’    // Add allowed MCP patterns\n   716â†’    for (const pattern of custom.allowedMcpPatterns) {\n   717â†’      const regex = validateRegex(pattern);\n   718â†’      if (regex) {\n   719â†’        merged.readOnlyMcpPatterns.push(regex);\n   720â†’      } else {\n   721â†’        debug(`[Permissions] Invalid MCP pattern, skipping: ${pattern}`);\n   722â†’      }\n   723â†’    }\n   724â†’\n   725â†’    // Add allowed API endpoints (fine-grained)\n   726â†’    for (const rule of custom.allowedApiEndpoints) {\n   727â†’      const pathRegex = validateRegex(rule.path);\n   728â†’      if (pathRegex) {\n   729â†’        merged.allowedApiEndpoints.push({\n   730â†’          method: rule.method,\n   731â†’          pathPattern: pathRegex,\n   732â†’        });\n   733â†’      } else {\n   734â†’        debug(`[Permissions] Invalid API endpoint path pattern, skipping: ${rule.path}`);\n   735â†’      }\n   736â†’    }\n   737â†’\n   738â†’    // Add allowed write paths (glob patterns, stored as strings)\n   739â†’    for (const pattern of custom.allowedWritePaths) {\n   740â†’      merged.allowedWritePaths.push(pattern);\n   741â†’    }\n   742â†’  }\n   743â†’\n   744â†’  /**\n   745â†’   * Apply source-specific config with auto-scoped MCP patterns.\n   746â†’   * MCP patterns in a source's permissions.json are automatically prefixed with\n   747â†’   * mcp__<sourceSlug>__ so they only apply to that source's tools.\n   748â†’   * This prevents cross-source leakage when using simple patterns like \"list\".\n   749â†’   */\n   750â†’  private applySourceConfig(\n   751â†’    merged: MergedPermissionsConfig,\n   752â†’    custom: PermissionsCustomConfig,\n   753â†’    sourceSlug: string\n   754â†’  ): void {\n   755â†’    // Write paths - apply normally (global effect)\n   756â†’    for (const pattern of custom.allowedWritePaths) {\n   757â†’      merged.allowedWritePaths.push(pattern);\n   758â†’    }\n   759â†’\n   760â†’    // MCP 