{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_vrtx_01PGnDQvHAu72snpj9GG6mWJ","type":"tool_result","content":"     1â†’/**\n     2â†’ * Safe Mode Configuration\n     3â†’ *\n     4â†’ * Allows customization of Safe Mode rules per workspace and per source.\n     5â†’ * Users can create permissions.json files to extend the default rules.\n     6â†’ *\n     7â†’ * File locations:\n     8â†’ * - Workspace: ~/.craft-agent/workspaces/{slug}/permissions.json\n     9â†’ * - Per-source: ~/.craft-agent/workspaces/{slug}/sources/{sourceSlug}/permissions.json\n    10â†’ *\n    11â†’ * Rules are additive - custom configs extend the defaults (more permissive).\n    12â†’ */\n    13â†’\n    14â†’import { existsSync, readFileSync, mkdirSync, writeFileSync } from 'fs';\n    15â†’import { join } from 'path';\n    16â†’import { debug } from '../utils/debug.ts';\n    17â†’import { CONFIG_DIR } from '../config/paths.ts';\n    18â†’import { getSourcePath } from '../sources/storage.ts';\n    19â†’import {\n    20â†’  SAFE_MODE_CONFIG,\n    21â†’  PermissionsConfigSchema,\n    22â†’  type ApiEndpointRule,\n    23â†’  type PermissionsConfigFile,\n    24â†’  type CompiledApiEndpointRule,\n    25â†’  type CompiledBashPattern,\n    26â†’  type PermissionPaths,\n    27â†’} from './mode-types.ts';\n    28â†’\n    29â†’// ============================================================\n    30â†’// App-level Permissions Directory\n    31â†’// ============================================================\n    32â†’\n    33â†’const APP_PERMISSIONS_DIR = join(CONFIG_DIR, 'permissions');\n    34â†’\n    35â†’/**\n    36â†’ * Get the app-level permissions directory.\n    37â†’ * Default permissions are stored at ~/.craft-agent/permissions/\n    38â†’ */\n    39â†’export function getAppPermissionsDir(): string {\n    40â†’  return APP_PERMISSIONS_DIR;\n    41â†’}\n    42â†’\n    43â†’/**\n    44â†’ * Ensure default permissions file exists.\n    45â†’ * Copies bundled default.json from the provided directory on first run.\n    46â†’ * Only copies if file doesn't exist (preserves user edits).\n    47â†’ * @param bundledPermissionsDir - Path to bundled permissions (e.g., Electron's resources/permissions)\n    48â†’ * @param language - Language code (e.g., 'en', 'zh-CN') for selecting the appropriate template\n    49â†’ */\n    50â†’export function ensureDefaultPermissions(bundledPermissionsDir?: string, language?: string): void {\n    51â†’  const permissionsDir = getAppPermissionsDir();\n    52â†’\n    53â†’  // Create permissions directory if it doesn't exist\n    54â†’  if (!existsSync(permissionsDir)) {\n    55â†’    mkdirSync(permissionsDir, { recursive: true });\n    56â†’  }\n    57â†’\n    58â†’  // If no bundled permissions directory provided, just ensure the directory exists\n    59â†’  if (!bundledPermissionsDir || !existsSync(bundledPermissionsDir)) {\n    60â†’    return;\n    61â†’  }\n    62â†’\n    63â†’  // Copy default.json if it doesn't exist in app permissions dir\n    64â†’  const destPath = join(permissionsDir, 'default.json');\n    65â†’  if (!existsSync(destPath)) {\n    66â†’    const templateFile = language ? `default.${language}.json` : 'default.en.json';\n    67â†’    const srcPath = join(bundledPermissionsDir, templateFile);\n    68â†’\n    69â†’    // Fallback to English if the language-specific template doesn't exist\n    70â†’    const finalSrcPath = existsSync(srcPath)\n    71â†’      ? srcPath\n    72â†’      : join(bundledPermissionsDir, 'default.en.json');\n    73â†’\n    74â†’    if (existsSync(finalSrcPath)) {\n    75â†’      try {\n    76â†’        const content = readFileSync(finalSrcPath, 'utf-8');\n    77â†’        writeFileSync(destPath, content, 'utf-8');\n    78â†’        debug('[Permissions] Copied bundled permissions template to', destPath);\n    79â†’      } catch (error) {\n    80â†’        debug('[Permissions] Error copying bundled permissions template:', error);\n    81â†’      }\n    82â†’    }\n    83â†’  }\n    84â†’}\n    85â†’\n    86â†’/**\n    87â†’ * Update default permissions file to match the current language.\n    88â†’ * Preserves user customizations while updating system default comments.\n    89â†’ * @param bundledPermissionsDir - Path to bundled permissions templates\n    90â†’ * @param language - Target language code (e.g., 'en', 'zh-CN')\n    91â†’ */\n    92â†’export function updateDefaultPermissionsLanguage(bundledPermissionsDir: string, language: string): void {\n    93â†’  const permissionsDir = getAppPermissionsDir();\n    94â†’  const destPath = join(permissionsDir, 'default.json');\n    95â†’\n    96â†’  if (!existsSync(bundledPermissionsDir)) {\n    97â†’    debug('[Permissions] Bundled permissions directory not found:', bundledPermissionsDir);\n    98â†’    return;\n    99â†’  }\n   100â†’\n   101â†’  // Load the new language template\n   102â†’  const templateFile = `default.${language}.json`;\n   103â†’  const templatePath = join(bundledPermissionsDir, templateFile);\n   104â†’\n   105â†’  // Fallback to English if the language-specific template doesn't exist\n   106â†’  const finalTemplatePath = existsSync(templatePath)\n   107â†’    ? templatePath\n   108â†’    : join(bundledPermissionsDir, 'default.en.json');\n   109â†’\n   110â†’  if (!existsSync(finalTemplatePath)) {\n   111â†’    debug('[Permissions] Template not found:', finalTemplatePath);\n   112â†’    return;\n   113â†’  }\n   114â†’\n   115â†’  try {\n   116â†’    // Load existing user config (if any)\n   117â†’    let userConfig: PermissionsConfigFile | null = null;\n   118â†’    if (existsSync(destPath)) {\n   119â†’      const existingContent = readFileSync(destPath, 'utf-8');\n   120â†’      userConfig = parsePermissionsJson(existingContent);\n   121â†’    }\n   122â†’\n   123â†’    // Load the new template\n   124â†’    const templateContent = readFileSync(finalTemplatePath, 'utf-8');\n   125â†’    const parsedTemplate = JSON.parse(templateContent);\n   126â†’    const templateResult = PermissionsConfigSchema.safeParse(parsedTemplate);\n   127â†’\n   128â†’    if (!templateResult.success) {\n   129â†’      debug('[Permissions] Invalid template format:', templateResult.error.issues);\n   130â†’      return;\n   131â†’    }\n   132â†’\n   133â†’    const templateConfig: PermissionsConfigFile = templateResult.data;\n   134â†’\n   135â†’    // Merge user customizations with new template\n   136â†’    const mergedConfig = mergePermissionsConfigs(templateConfig, userConfig);\n   137â†’\n   138â†’    // Write the merged config\n   139â†’    writeFileSync(destPath, JSON.stringify(mergedConfig, null, 2), 'utf-8');\n   140â†’    debug('[Permissions] Updated default permissions to language:', language);\n   141â†’  } catch (error) {\n   142â†’    debug('[Permissions] Error updating permissions language:', error);\n   143â†’  }\n   144â†’}\n   145â†’\n   146â†’/**\n   147â†’ * Check if a comment is user-modified (starts with [USER] prefix)\n   148â†’ */\n   149â†’function isUserComment(comment: string | undefined): boolean {\n   150â†’  return comment !== undefined && comment.trim().startsWith('[USER]');\n   151â†’}\n   152â†’\n   153â†’/**\n   154â†’ * Merge user customizations into a new template.\n   155â†’ * Preserves user-added patterns while updating system default comments.\n   156â†’ * Protects user-modified comments from being overwritten.\n   157â†’ */\n   158â†’function mergePermissionsConfigs(\n   159â†’  template: PermissionsConfigFile,\n   160â†’  userConfig: PermissionsConfigFile | null\n   161â†’): PermissionsConfigFile {\n   162â†’  if (!userConfig) {\n   163â†’    return template;\n   164â†’  }\n   165â†’\n   166â†’  // Start with the template as base\n   167â†’  const merged: PermissionsConfigFile = JSON.parse(JSON.stringify(template));\n   168â†’\n   169â†’  // Helper to check if a pattern exists in template\n   170â†’  const patternExistsInTemplate = (\n   171â†’    pattern: string,\n   172â†’    templateArray: Array<string | { pattern: string; comment?: string }>\n   173â†’  ): boolean => {\n   174â†’    return templateArray.some(item => {\n   175â†’      const itemPattern = typeof item === 'string' ? item : item.pattern;\n   176â†’      return itemPattern === pattern;\n   177â†’    });\n   178â†’  };\n   179â†’\n   180â†’  // Helper to merge patterns with user comment protection\n   181â†’  const mergePatterns = (\n   182â†’    templatePatterns: Array<string | { pattern: string; comment?: string; userModified?: boolean }> | undefined,\n   183â†’    userPatterns: Array<string | { pattern: string; comment?: string; userModified?: boolean }> | undefined\n   184â†’  ): Array<string | { pattern: string; comment?: string; userModified?: boolean }> => {\n   185â†’    const result = [...(templatePatterns || [])];\n   186â†’\n   187â†’    if (!userPatterns) return result;\n   188â†’\n   189â†’    // Process each user pattern\n   190â†’    userPatterns.forEach(userItem => {\n   191â†’      const userPattern = typeof userItem === 'string' ? userItem : userItem.pattern;\n   192â†’\n   193â†’      // Find if this pattern exists in template\n   194â†’      const templateIndex = result.findIndex(item => {\n   195â†’        const itemPattern = typeof item === 'string' ? item : item.pattern;\n   196â†’        return itemPattern === userPattern;\n   197â†’      });\n   198â†’\n   199â†’      if (templateIndex === -1) {\n   200â†’        // Pattern not in template - user added it, preserve completely\n   201â†’        result.push(userItem);\n   202â†’      } else if (typeof userItem !== 'string') {\n   203â†’        // Pattern exists in template - check if user modified the comment\n   204â†’        const isModified = userItem.userModified || isUserComment(userItem.comment);\n   205â†’\n   206â†’        if (isModified) {\n   207â†’          // User modified this rule - preserve the user's comment\n   208â†’          result[templateIndex] = {\n   209â†’            pattern: userPattern,\n   210â†’            comment: userItem.comment,\n   211â†’            userModified: true,\n   212â†’          };\n   213â†’        }\n   214â†’        // Otherwise, use template's comment (language update scenario)\n   215â†’      }\n   216â†’    });\n   217â†’\n   218â†’    return result;\n   219â†’  };\n   220â†’\n   221â†’  // Merge bash patterns with user comment protection\n   222â†’  if (userConfig.allowedBashPatterns || template.allowedBashPatterns) {\n   223â†’    merged.allowedBashPatterns = mergePatterns(\n   224â†’      template.allowedBashPatterns,\n   225â†’      userConfig.allowedBashPatterns\n   226â†’    );\n   227â†’  }\n   228â†’\n   229â†’  // Merge MCP patterns with user comment protection\n   230â†’  if (userConfig.allowedMcpPatterns || template.allowedMcpPatterns) {\n   231â†’    merged.allowedMcpPatterns = mergePatterns(\n   232â†’      template.allowedMcpPatterns,\n   233â†’      userConfig.allowedMcpPatterns\n   234â†’    );\n   235â†’  }\n   236â†’\n   237â†’  // Merge write paths with user comment protection\n   238â†’  if (userConfig.allowedWritePaths || template.allowedWritePaths) {\n   239â†’    merged.allowedWritePaths = mergePatterns(\n   240â†’      template.allowedWritePaths,\n   241â†’      userConfig.allowedWritePaths\n   242â†’    );\n   243â†’  }\n   244â†’\n   245â†’  // Merge API endpoints - preserve user additions\n   246â†’  if (userConfig.allowedApiEndpoints && userConfig.allowedApiEndpoints.length > 0) {\n   247â†’    merged.allowedApiEndpoints = [\n   248â†’      ...(template.allowedApiEndpoints || []),\n   249â†’      ...userConfig.allowedApiEndpoints\n   250â†’    ];\n   251â†’  }\n   252â†’\n   253â†’  return merged;\n   254â†’}\n   255â†’\n   256â†’/**\n   257â†’ * Load default permissions from ~/.craft-agent/permissions/default.json\n   258â†’ * Returns null if file doesn't exist or is invalid.\n   259â†’ */\n   260â†’export function loadDefaultPermissions(): PermissionsCustomConfig | null {\n   261â†’  const defaultPath = join(getAppPermissionsDir(), 'default.json');\n   262â†’  if (!existsSync(defaultPath)) {\n   263â†’    debug('[Permissions] No default.json found at', defaultPath);\n   264â†’    return null;\n   265â†’  }\n   266â†’\n   267â†’  try {\n   268â†’    const content = readFileSync(defaultPath, 'utf-8');\n   269â†’    const config = parsePermissionsJson(content);\n   270â†’    debug('[Permissions] Loaded default permissions from', defaultPath);\n   271â†’    return config;\n   272â†’  } catch (error) {\n   273â†’    debug('[Permissions] Error loading default permissions:', error);\n   274â†’    return null;\n   275â†’  }\n   276â†’}\n   277â†’\n   278â†’// Re-export types from mode-types for external consumers\n   279â†’export {\n   280â†’  PermissionsConfigSchema,\n   281â†’  type ApiEndpointRule,\n   282â†’  type PermissionsConfigFile,\n   283â†’  type CompiledApiEndpointRule,\n   284â†’  type CompiledBashPattern,\n   285â†’  type PermissionPaths,\n   286â†’};\n   287â†’\n   288â†’// ============================================================\n   289â†’// Types\n   290â†’// ============================================================\n   291â†’\n   292â†’/**\n   293â†’ * Pattern entry with optional comment for error messages.\n   294â†’ * Preserves the comment from permissions.json so we can show helpful hints.\n   295â†’ */\n   296â†’export interface PatternWithComment {\n   297â†’  pattern: string;\n   298â†’  comment?: string;\n   299â†’  userModified?: boolean;\n   300â†’}\n   301â†’\n   302â†’/**\n   303â†’ * Parsed and normalized permissions configuration\n   304â†’ *\n   305â†’ * Note: blockedTools (Write, Edit, MultiEdit, NotebookEdit) are hardcoded in\n   306â†’ * SAFE_MODE_CONFIG and not configurable here - they're fundamental write\n   307â†’ * operations that must always be blocked in Explore mode.\n   308â†’ */\n   309â†’export interface PermissionsCustomConfig {\n   310â†’  /** Additional bash patterns to allow (with optional comments for error messages) */\n   311â†’  allowedBashPatterns: PatternWithComment[];\n   312â†’  /** Additional MCP patterns to allow (as regex strings) */\n   313â†’  allowedMcpPatterns: string[];\n   314â†’  /** API endpoint rules for fine-grained control */\n   315â†’  allowedApiEndpoints: ApiEndpointRule[];\n   316â†’  /** File paths to allow writes in Explore mode (glob pattern strings) */\n   317â†’  allowedWritePaths: string[];\n   318â†’}\n   319â†’\n   320â†’/**\n   321â†’ * Merged permissions config for runtime use\n   322â†’ */\n   323â†’export interface MergedPermissionsConfig {\n   324â†’  /** Blocked tools (Write, Edit, MultiEdit, NotebookEdit) - hardcoded, not configurable */\n   325â†’  blockedTools: Set<string>;\n   326â†’  /** Read-only bash patterns with metadata for helpful error messages */\n   327â†’  readOnlyBashPatterns: CompiledBashPattern[];\n   328â†’  readOnlyMcpPatterns: RegExp[];\n   329â†’  /** Fine-grained API endpoint rules */\n   330â†’  allowedApiEndpoints: CompiledApiEndpointRule[];\n   331â†’  /** File paths allowed for writes in Explore mode (glob patterns) */\n   332â†’  allowedWritePaths: string[];\n   333â†’  /** Display name for error messages */\n   334â†’  displayName: string;\n   335â†’  /** Keyboard shortcut hint */\n   336â†’  shortcutHint: string;\n   337â†’  /** Paths to permission files for actionable error messages */\n   338â†’  permissionPaths?: PermissionPaths;\n   339â†’}\n   340â†’\n   341â†’/**\n   342â†’ * Context for permissions checking (includes workspace/source/agent info)\n   343â†’ */\n   344â†’export interface PermissionsContext {\n   345â†’  workspaceRootPath: string;\n   346â†’  /** Active source slugs for source-specific rules */\n   347â†’  activeSourceSlugs?: string[];\n   348â†’}\n   349â†’\n   350â†’// ============================================================\n   351â†’// JSON Parser\n   352â†’// ============================================================\n   353â†’\n   354â†’/**\n   355â†’ * Parse and validate permissions.json file\n   356â†’ */\n   357â†’export function parsePermissionsJson(content: string): PermissionsCustomConfig {\n   358â†’  const emptyConfig: PermissionsCustomConfig = {\n   359â†’    allowedBashPatterns: [],\n   360â†’    allowedMcpPatterns: [],\n   361â†’    allowedApiEndpoints: [],\n   362â†’    allowedWritePaths: [],\n   363â†’  };\n   364â†’\n   365â†’  try {\n   366â†’    const json = JSON.parse(content);\n   367â†’    const result = PermissionsConfigSchema.safeParse(json);\n   368â†’\n   369â†’    if (!result.success) {\n   370â†’      debug('[SafeMode] Validation errors:', result.error.issues);\n   371â†’      // Log specific errors for debugging\n   372â†’      for (const issue of result.error.issues) {\n   373â†’        debug(`[SafeMode]   - ${issue.path.join('.')}: ${issue.message}`);\n   374â†’      }\n   375â†’      return emptyConfig;\n   376â†’    }\n   377â†’\n   378â†’    const data = result.data;\n   379â†’\n   380â†’    // Normalize patterns (extract string from pattern objects, but NOT for bash - preserve comments)\n   381â†’    const normalizePatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): string[] => {\n   382â†’      if (!patterns) return [];\n   383â†’      return patterns.map(p => typeof p === 'string' ? p : p.pattern);\n   384â†’    };\n   385â†’\n   386â†’    // For bash patterns, preserve comments for helpful error messages\n   387â†’    const normalizeBashPatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): PatternWithComment[] => {\n   388â†’      if (!patterns) return [];\n   389â†’      return patterns.map(p => {\n   390â†’        if (typeof p === 'string') {\n   391â†’          return { pattern: p };\n   392â†’        }\n   393â†’        return { pattern: p.pattern, comment: p.comment };\n   394â†’      });\n   395â†’    };\n   396â†’\n   397â†’    return {\n   398â†’      allowedBashPatterns: normalizeBashPatterns(data.allowedBashPatterns),\n   399â†’      allowedMcpPatterns: normalizePatterns(data.allowedMcpPatterns),\n   400â†’      allowedApiEndpoints: data.allowedApiEndpoints ?? [],\n   401â†’      allowedWritePaths: normalizePatterns(data.allowedWritePaths),\n   402â†’    };\n   403â†’  } catch (error) {\n   404â†’    debug('[SafeMode] JSON parse error:', error);\n   405â†’    return emptyConfig;\n   406â†’  }\n   407â†’}\n   408â†’\n   409â†’/**\n   410â†’ * Validate a regex pattern string, return null if invalid\n   411â†’ */\n   412â†’function validateRegex(pattern: string): RegExp | null {\n   413â†’  try {\n   414â†’    return new RegExp(pattern);\n   415â†’  } catch {\n   416â†’    return null;\n   417â†’  }\n   418â†’}\n   419â†’\n   420â†’/**\n   421â†’ * Validate permissions config and return errors\n   422â†’ */\n   423â†’export function validatePermissionsConfig(config: PermissionsConfigFile): string[] {\n   424â†’  const errors: string[] = [];\n   425â†’\n   426â†’  // Validate regex patterns\n   427â†’  const checkPatterns = (patterns: Array<string | { pattern: string }> | undefined, name: string) => {\n   428â†’    if (!patterns) return;\n   429â†’    for (let i = 0; i < patterns.length; i++) {\n   430â†’      const p = patterns[i];\n   431â†’      if (!p) continue;\n   432â†’      const patternStr = typeof p === 'string' ? p : p.pattern;\n   433â†’      if (!validateRegex(patternStr)) {\n   434â†’        errors.push(`${name}[${i}]: Invalid regex pattern: ${patternStr}`);\n   435â†’      }\n   436â†’    }\n   437â†’  };\n   438â†’\n   439â†’  checkPatterns(config.allowedBashPatterns, 'allowedBashPatterns');\n   440â†’  checkPatterns(config.allowedMcpPatterns, 'allowedMcpPatterns');\n   441â†’\n   442â†’  // Validate API endpoint patterns\n   443â†’  if (config.allowedApiEndpoints) {\n   444â†’    for (let i = 0; i < config.allowedApiEndpoints.length; i++) {\n   445â†’      const rule = config.allowedApiEndpoints[i];\n   446â†’      if (rule && !validateRegex(rule.path)) {\n   447â†’        errors.push(`allowedApiEndpoints[${i}].path: Invalid regex pattern: ${rule.path}`);\n   448â†’      }\n   449â†’    }\n   450â†’  }\n   451â†’\n   452â†’  return errors;\n   453â†’}\n   454â†’\n   455â†’// ============================================================\n   456â†’// Storage Functions\n   457â†’// ============================================================\n   458â†’\n   459â†’/**\n   460â†’ * Get path to workspace permissions.json\n   461â†’ */\n   462â†’export function getWorkspacePermissionsPath(workspaceRootPath: string): string {\n   463â†’  return join(workspaceRootPath, 'permissions.json');\n   464â†’}\n   465â†’\n   466â†’/**\n   467â†’ * Get path to source permissions.json\n   468â†’ */\n   469â†’export function getSourcePermissionsPath(workspaceRootPath: string, sourceSlug: string): string {\n   470â†’  return join(getSourcePath(workspaceRootPath, sourceSlug), 'permissions.json');\n   471â†’}\n   472â†’\n   473â†’/**\n   474â†’ * Load workspace-level permissions config\n   475â†’ */\n   476â†’export function loadWorkspacePermissionsConfig(workspaceRootPath: string): PermissionsCustomConfig | null {\n   477â†’  const path = getWorkspacePermissionsPath(workspaceRootPath);\n   478â†’  if (!existsSync(path)) return null;\n   479â†’\n   480â†’  try {\n   481â†’    const content = readFileSync(path, 'utf-8');\n   482â†’    const config = parsePermissionsJson(content);\n   483â†’    debug(`[Permissions] Loaded workspace config from ${path}:`, config);\n   484â†’    return config;\n   485â†’  } catch (error) {\n   486â†’    debug(`[Permissions] Error loading workspace config:`, error);\n   487â†’    return null;\n   488â†’  }\n   489â†’}\n   490â†’\n   491â†’/**\n   492â†’ * Load source-level permissions config\n   493â†’ */\n   494â†’export function loadSourcePermissionsConfig(\n   495â†’  workspaceRootPath: string,\n   496â†’  sourceSlug: string\n   497â†’): PermissionsCustomConfig | null {\n   498â†’  const path = getSourcePermissionsPath(workspaceRootPath, sourceSlug);\n   499â†’  if (!existsSync(path)) return null;\n   500â†’\n   501â†’  try {\n   502â†’    const content = readFileSync(path, 'utf-8');\n   503â†’    const config = parsePermissionsJson(content);\n   504â†’    debug(`[Permissions] Loaded source config from ${path}:`, config);\n   505â†’    return config;\n   506â†’  } catch (error) {\n   507â†’    debug(`[Permissions] Error loading source config:`, error);\n   508â†’    return null;\n   509â†’  }\n   510â†’}\n   511â†’\n   512â†’// ============================================================\n   513â†’// API Endpoint Checking\n   514â†’// ============================================================\n   515â†’\n   516â†’/**\n   517â†’ * Check if an API call is allowed by endpoint rules\n   518â†’ */\n   519â†’export function isApiEndpointAllowed(\n   520â†’  method: string,\n   521â†’  path: string,\n   522â†’  config: MergedPermissionsConfig\n   523â†’): boolean {\n   524â†’  const upperMethod = method.toUpperCase();\n   525â†’\n   526â†’  // GET is always allowed\n   527â†’  if (upperMethod === 'GET') return true;\n   528â†’\n   529â†’  // Check fine-grained endpoint rules\n   530â†’  for (const rule of config.allowedApiEndpoints) {\n   531â†’    if (rule.method === upperMethod && rule.pathPattern.test(path)) {\n   532â†’      return true;\n   533â†’    }\n   534â†’  }\n   535â†’\n   536â†’  return false;\n   537â†’}\n   538â†’\n   539â†’// ============================================================\n   540â†’// Config Cache\n   541â†’// ============================================================\n   542â†’\n   543â†’/**\n   544â†’ * In-memory cache for parsed permissions configs\n   545â†’ * Invalidated on file changes via ConfigWatcher\n   546â†’ */\n   547â†’class PermissionsConfigCache {\n   548â†’  private workspaceConfigs: Map<string, PermissionsCustomConfig | null> = new Map();\n   549â†’  private sourceConfigs: Map<string, PermissionsCustomConfig | null> = new Map();\n   550â†’  private mergedConfigs: Map<string, MergedPermissionsConfig> = new Map();\n   551â†’\n   552â†’  // App-level default permissions (loaded from ~/.craft-agent/permissions/default.json)\n   553â†’  private defaultConfig: PermissionsCustomConfig | null | undefined = undefined; // undefined = not loaded yet\n   554â†’\n   555â†’  /**\n   556â†’   * Get or load app-level default permissions\n   557â†’   * These come from ~/.craft-agent/permissions/default.json\n   558â†’   */\n   559â†’  private getDefaultConfig(): PermissionsCustomConfig | null {\n   560â†’    if (this.defaultConfig === undefined) {\n   561â†’      this.defaultConfig = loadDefaultPermissions();\n   562â†’    }\n   563â†’    return this.defaultConfig;\n   564â†’  }\n   565â†’\n   566â†’  /**\n   567â†’   * Get or load workspace config\n   568â†’   */\n   569â†’  getWorkspaceConfig(workspaceRootPath: string): PermissionsCustomConfig | null {\n   570â†’    if (!this.workspaceConfigs.has(workspaceRootPath)) {\n   571â†’      this.workspaceConfigs.set(workspaceRootPath, loadWorkspacePermissionsConfig(workspaceRootPath));\n   572â†’    }\n   573â†’    return this.workspaceConfigs.get(workspaceRootPath) ?? null;\n   574â†’  }\n   575â†’\n   576â†’  /**\n   577â†’   * Get or load source config\n   578â†’   */\n   579â†’  getSourceConfig(workspaceRootPath: string, sourceSlug: string): PermissionsCustomConfig | null {\n   580â†’    const key = `${workspaceRootPath}::${sourceSlug}`;\n   581â†’    if (!this.sourceConfigs.has(key)) {\n   582â†’      this.sourceConfigs.set(key, loadSourcePermissionsConfig(workspaceRootPath, sourceSlug));\n   583â†’    }\n   584â†’    return this.sourceConfigs.get(key) ?? null;\n   585â†’  }\n   586â†’\n   587â†’  /**\n   588â†’   * Invalidate app-level default permissions (called by ConfigWatcher)\n   589â†’   * This clears all merged configs since defaults affect everything\n   590â†’   */\n   591â†’  invalidateDefaults(): void {\n   592â†’    debug('[Permissions] Invalidating app