gs.delete(key);\n   632â†’      }\n   633â†’    }\n   634â†’  }\n   635â†’\n   636â†’\n   637â†’  /**\n   638â†’   * Get merged config for a context (workspace + active sources)\n   639â†’   * Uses additive merging: custom configs extend defaults\n   640â†’   */\n   641â†’  getMergedConfig(context: PermissionsContext): MergedPermissionsConfig {\n   642â†’    const cacheKey = this.buildCacheKey(context);\n   643â†’\n   644â†’    if (!this.mergedConfigs.has(cacheKey)) {\n   645â†’      const merged = this.buildMergedConfig(context);\n   646â†’      this.mergedConfigs.set(cacheKey, merged);\n   647â†’    }\n   648â†’\n   649â†’    return this.mergedConfigs.get(cacheKey)!;\n   650â†’  }\n   651â†’\n   652â†’  private buildMergedConfig(context: PermissionsContext): MergedPermissionsConfig {\n   653â†’    const defaults = SAFE_MODE_CONFIG;\n   654â†’\n   655â†’    // Start with hardcoded fallback defaults (blocked tools are fixed, display settings)\n   656â†’    // blockedTools (Write, Edit, MultiEdit, NotebookEdit) come from SAFE_MODE_CONFIG\n   657â†’    // and cannot be modified via permissions.json\n   658â†’    const merged: MergedPermissionsConfig = {\n   659â†’      blockedTools: new Set(defaults.blockedTools),\n   660â†’      readOnlyBashPatterns: [...defaults.readOnlyBashPatterns],\n   661â†’      readOnlyMcpPatterns: [...defaults.readOnlyMcpPatterns],\n   662â†’      allowedApiEndpoints: [],\n   663â†’      allowedWritePaths: [],\n   664â†’      displayName: defaults.displayName,\n   665â†’      shortcutHint: defaults.shortcutHint,\n   666â†’      // Add permission file paths for actionable error messages\n   667â†’      permissionPaths: {\n   668â†’        workspacePath: getWorkspacePermissionsPath(context.workspaceRootPath),\n   669â†’        appDefaultPath: join(getAppPermissionsDir(), 'default.json'),\n   670â†’        docsPath: join(CONFIG_DIR, 'docs', 'permissions.md'),\n   671â†’      },\n   672â†’    };\n   673â†’\n   674â†’    // Load and apply app-level default permissions from JSON\n   675â†’    // This is where the actual bash/MCP patterns come from\n   676â†’    const defaultConfig = this.getDefaultConfig();\n   677â†’    if (defaultConfig) {\n   678â†’      this.applyDefaultConfig(merged, defaultConfig);\n   679â†’    }\n   680â†’\n   681â†’    // Add workspace-level customizations\n   682â†’    const wsConfig = this.getWorkspaceConfig(context.workspaceRootPath);\n   683â†’    if (wsConfig) {\n   684â†’      this.applyCustomConfig(merged, wsConfig);\n   685â†’    }\n   686â†’\n   687â†’    // Add source-level customizations (additive, with auto-scoped MCP patterns)\n   688â†’    if (context.activeSourceSlugs) {\n   689â†’      for (const sourceSlug of context.activeSourceSlugs) {\n   690â†’        const srcConfig = this.getSourceConfig(context.workspaceRootPath, sourceSlug);\n   691â†’        if (srcConfig) {\n   692â†’          // Use applySourceConfig which auto-scopes MCP patterns to this source\n   693â†’          this.applySourceConfig(merged, srcConfig, sourceSlug);\n   694â†’        }\n   695â†’      }\n   696â†’    }\n   697â†’\n   698â†’    return merged;\n   699â†’  }\n   700â†’\n   701â†’  /**\n   702â†’   * Apply app-level default config (from default.json)\n   703â†’   * This adds bash/MCP patterns from the JSON config. Blocked tools are hardcoded\n   704â†’   * in SAFE_MODE_CONFIG and not loaded from JSON.\n   705â†’   */\n   706â†’  private applyDefaultConfig(merged: MergedPermissionsConfig, config: PermissionsCustomConfig): void {\n   707â†’    // Add allowed bash patterns (as CompiledBashPattern with metadata for error messages)\n   708â†’    for (const patternEntry of config.allowedBashPatterns) {\n   709â†’      const regex = validateRegex(patternEntry.pattern);\n   710â†’      if (regex) {\n   711â†’        merged.readOnlyBashPatterns.push({\n   712â†’          regex,\n   713â†’          source: patternEntry.pattern,\n   714â†’          comment: patternEntry.comment,\n   715â†’        });\n   716â†’      } else {\n   717â†’        debug(`[Permissions] Invalid default bash pattern, skipping: ${patternEntry.pattern}`);\n   718â†’      }\n   719â†’    }\n   720â†’\n   721â†’    // Add allowed MCP patterns\n   722â†’    for (const pattern of config.allowedMcpPatterns) {\n   723â†’      const regex = validateRegex(pattern);\n   724â†’      if (regex) {\n   725â†’        merged.readOnlyMcpPatterns.push(regex);\n   726â†’      } else {\n   727â†’        debug(`[Permissions] Invalid default MCP pattern, skipping: ${pattern}`);\n   728â†’      }\n   729â†’    }\n   730â†’\n   731â†’    // Add allowed API endpoints\n   732â†’    for (const rule of config.allowedApiEndpoints) {\n   733â†’      const pathRegex = validateRegex(rule.path);\n   734â†’      if (pathRegex) {\n   735â†’        merged.allowedApiEndpoints.push({\n   736â†’          method: rule.method,\n   737â†’          pathPattern: pathRegex,\n   738â†’        });\n   739â†’      }\n   740â†’    }\n   741â†’\n   742â†’    // Add allowed write paths\n   743â†’    for (const pattern of config.allowedWritePaths) {\n   744â†’      merged.allowedWritePaths.push(pattern);\n   745â†’    }\n   746â†’  }\n   747â†’\n   748â†’  private applyCustomConfig(merged: MergedPermissionsConfig, custom: PermissionsCustomConfig): void {\n   749â†’    // Add allowed bash patterns (making config more permissive)\n   750â†’    for (const patternEntry of custom.allowedBashPatterns) {\n   751â†’      const regex = validateRegex(patternEntry.pattern);\n   752â†’      if (regex) {\n   753â†’        merged.readOnlyBashPatterns.push({\n   754â†’          regex,\n   755â†’          source: patternEntry.pattern,\n   756â†’          comment: patternEntry.comment,\n   757â†’        });\n   758â†’      } else {\n   759â†’        debug(`[Permissions] Invalid bash pattern, skipping: ${patternEntry.pattern}`);\n   760â†’      }\n   761â†’    }\n   762â†’\n   763â†’    // Add allowed MCP patterns\n   764â†’    for (const pattern of custom.allowedMcpPatterns) {\n   765â†’      const regex = validateRegex(pattern);\n   766â†’      if (regex) {\n   767â†’        merged.readOnlyMcpPatterns.push(regex);\n   768â†’      } else {\n   769â†’        debug(`[Permissions] Invalid MCP pattern, skipping: ${pattern}`);\n   770â†’      }\n   771â†’    }\n   772â†’\n   773â†’    // Add allowed API endpoints (fine-grained)\n   774â†’    for (const rule of custom.allowedApiEndpoints) {\n   775â†’      const pathRegex = validateRegex(rule.path);\n   776â†’      if (pathRegex) {\n   777â†’        merged.allowedApiEndpoints.push({\n   778â†’          method: rule.method,\n   779â†’          pathPattern: pathRegex,\n   780â†’        });\n   781â†’      } else {\n   782â†’        debug(`[Permissions] Invalid API endpoint path pattern, skipping: ${rule.path}`);\n   783â†’      }\n   784â†’    }\n   785â†’\n   786â†’    // Add allowed write paths (glob patterns, stored as strings)\n   787â†’    for (const pattern of custom.allowedWritePaths) {\n   788â†’      merged.allowedWritePaths.push(pattern);\n   789â†’    }\n   790â†’  }\n   791â†’\n   792â†’  /**\n   793â†’   * Apply source-specific config with auto-scoped MCP patterns.\n   794â†’   * MCP patterns in a source's permissions.json are automatically prefixed with\n   795â†’   * mcp__<sourceSlug>__ so they only apply to that source's tools.\n   796â†’   * This prevents cross-source leakage when using simple patterns like \"list\".\n   797â†’   */\n   798â†’  private applySourceConfig(\n   799â†’    merged: MergedPermissionsConfig,\n   800â†’    custom: PermissionsCustomConfig,\n   801â†’    sourceSlug: string\n   802â†’  ): void {\n   803â†’    // Write paths - apply normally (global effect)\n   804â†’    for (const pattern of custom.allowedWritePaths) {\n   805â†’      merged.allowedWritePaths.push(pattern);\n   806â†’    }\n   807â†’\n   808â†’    // MCP patterns - AUTO-SCOPE to this source\n   809â†’    // User writes: \"list\" â†’ becomes: \"mcp__<sourceSlug>__.*list\"\n   810â†’    // This ensures patterns only match tools from THIS source\n   811â†’    for (const pattern of custom.allowedMcpPatterns) {\n   812â†’      const scopedPattern = `mcp__${sourceSlug}__.*${pattern}`;\n   813â†’      const regex = validateRegex(scopedPattern);\n   814â†’      if (regex) {\n   815â†’        merged.readOnlyMcpPatterns.push(regex);\n   816â†’        debug(`[Permissions] Scoped MCP pattern for ${sourceSlug}: ${pattern} â†’ ${scopedPattern}`);\n   817â†’      } else {\n   818â†’        debug(`[Permissions] Invalid MCP pattern after scoping, skipping: ${scopedPattern}`);\n   819â†’      }\n   820â†’    }\n   821â†’\n   822â†’    // Bash patterns - apply normally (not source-specific)\n   823â†’    for (const patternEntry of custom.allowedBashPatterns) {\n   824â†’      const regex = validateRegex(patternEntry.pattern);\n   825â†’      if (regex) {\n   826â†’        merged.readOnlyBashPatterns.push({\n   827â†’          regex,\n   828â†’          source: patternEntry.pattern,\n   829â†’          comment: patternEntry.comment,\n   830â†’        });\n   831â†’      } else {\n   832â†’        debug(`[Permissions] Invalid bash pattern, skipping: ${patternEntry.pattern}`);\n   833â†’      }\n   834â†’    }\n   835â†’\n   836â†’    // API endpoints - apply normally (API tools are already source-scoped as api_<slug>)\n   837â†’    for (const rule of custom.allowedApiEndpoints) {\n   838â†’      const pathRegex = validateRegex(rule.path);\n   839â†’      if (pathRegex) {\n   840â†’        merged.allowedApiEndpoints.push({\n   841â†’          method: rule.method,\n   842â†’          pathPattern: pathRegex,\n   843â†’        });\n   844â†’      } else {\n   845â†’        debug(`[Permissions] Invalid API endpoint path pattern, skipping: ${rule.path}`);\n   846â†’      }\n   847â†’    }\n   848â†’  }\n   849â†’\n   850â†’  private buildCacheKey(context: PermissionsContext): string {\n   851â†’    const sources = context.activeSourceSlugs?.sort().join(',') ?? '';\n   852â†’    return `${context.workspaceRootPath}::${sources}`;\n   853â†’  }\n   854â†’\n   855â†’  /**\n   856â†’   * Clear all cached configs\n   857â†’   */\n   858â†’  clear(): void {\n   859â†’    this.defaultConfig = undefined;\n   860â†’    this.workspaceConfigs.clear();\n   861â†’    this.sourceConfigs.clear();\n   862â†’    this.mergedConfigs.clear();\n   863â†’  }\n   864â†’}\n   865â†’\n   866â†’// Singleton instance\n   867â†’export const permissionsConfigCache = new PermissionsConfigCache();\n   868â†’\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"e6787c08-bcbd-42b7-952a-5407060e8fc8","uuid":"503a4629-346c-45fa-9279-130933286c29","tool_use_result":{"type":"text","file":{"filePath":"/Users/hubo/Tools/craft-agents-oss/packages/shared/src/agent/permissions-config.ts","content":"/**\n * Safe Mode Configuration\n *\n * Allows customization of Safe Mode rules per workspace and per source.\n * Users can create permissions.json files to extend the default rules.\n *\n * File locations:\n * - Workspace: ~/.craft-agent/workspaces/{slug}/permissions.json\n * - Per-source: ~/.craft-agent/workspaces/{slug}/sources/{sourceSlug}/permissions.json\n *\n * Rules are additive - custom configs extend the defaults (more permissive).\n */\n\nimport { existsSync, readFileSync, mkdirSync, writeFileSync } from 'fs';\nimport { join } from 'path';\nimport { debug } from '../utils/debug.ts';\nimport { CONFIG_DIR } from '../config/paths.ts';\nimport { getSourcePath } from '../sources/storage.ts';\nimport {\n  SAFE_MODE_CONFIG,\n  PermissionsConfigSchema,\n  type ApiEndpointRule,\n  type PermissionsConfigFile,\n  type CompiledApiEndpointRule,\n  type CompiledBashPattern,\n  type PermissionPaths,\n} from './mode-types.ts';\n\n// ============================================================\n// App-level Permissions Directory\n// ============================================================\n\nconst APP_PERMISSIONS_DIR = join(CONFIG_DIR, 'permissions');\n\n/**\n * Get the app-level permissions directory.\n * Default permissions are stored at ~/.craft-agent/permissions/\n */\nexport function getAppPermissionsDir(): string {\n  return APP_PERMISSIONS_DIR;\n}\n\n/**\n * Ensure default permissions file exists.\n * Copies bundled default.json from the provided directory on first run.\n * Only copies if file doesn't exist (preserves user edits).\n * @param bundledPermissionsDir - Path to bundled permissions (e.g., Electron's resources/permissions)\n * @param language - Language code (e.g., 'en', 'zh-CN') for selecting the appropriate template\n */\nexport function ensureDefaultPermissions(bundledPermissionsDir?: string, language?: string): void {\n  const permissionsDir = getAppPermissionsDir();\n\n  // Create permissions directory if it doesn't exist\n  if (!existsSync(permissionsDir)) {\n    mkdirSync(permissionsDir, { recursive: true });\n  }\n\n  // If no bundled permissions directory provided, just ensure the directory exists\n  if (!bundledPermissionsDir || !existsSync(bundledPermissionsDir)) {\n    return;\n  }\n\n  // Copy default.json if it doesn't exist in app permissions dir\n  const destPath = join(permissionsDir, 'default.json');\n  if (!existsSync(destPath)) {\n    const templateFile = language ? `default.${language}.json` : 'default.en.json';\n    const srcPath = join(bundledPermissionsDir, templateFile);\n\n    // Fallback to English if the language-specific template doesn't exist\n    const finalSrcPath = existsSync(srcPath)\n      ? srcPath\n      : join(bundledPermissionsDir, 'default.en.json');\n\n    if (existsSync(finalSrcPath)) {\n      try {\n        const content = readFileSync(finalSrcPath, 'utf-8');\n        writeFileSync(destPath, content, 'utf-8');\n        debug('[Permissions] Copied bundled permissions template to', destPath);\n      } catch (error) {\n        debug('[Permissions] Error copying bundled permissions template:', error);\n      }\n    }\n  }\n}\n\n/**\n * Update default permissions file to match the current language.\n * Preserves user customizations while updating system default comments.\n * @param bundledPermissionsDir - Path to bundled permissions templates\n * @param language - Target language code (e.g., 'en', 'zh-CN')\n */\nexport function updateDefaultPermissionsLanguage(bundledPermissionsDir: string, language: string): void {\n  const permissionsDir = getAppPermissionsDir();\n  const destPath = join(permissionsDir, 'default.json');\n\n  if (!existsSync(bundledPermissionsDir)) {\n    debug('[Permissions] Bundled permissions directory not found:', bundledPermissionsDir);\n    return;\n  }\n\n  // Load the new language template\n  const templateFile = `default.${language}.json`;\n  const templatePath = join(bundledPermissionsDir, templateFile);\n\n  // Fallback to English if the language-specific template doesn't exist\n  const finalTemplatePath = existsSync(templatePath)\n    ? templatePath\n    : join(bundledPermissionsDir, 'default.en.json');\n\n  if (!existsSync(finalTemplatePath)) {\n    debug('[Permissions] Template not found:', finalTemplatePath);\n    return;\n  }\n\n  try {\n    // Load existing user config (if any)\n    let userConfig: PermissionsConfigFile | null = null;\n    if (existsSync(destPath)) {\n      const existingContent = readFileSync(destPath, 'utf-8');\n      userConfig = parsePermissionsJson(existingContent);\n    }\n\n    // Load the new template\n    const templateContent = readFileSync(finalTemplatePath, 'utf-8');\n    const parsedTemplate = JSON.parse(templateContent);\n    const templateResult = PermissionsConfigSchema.safeParse(parsedTemplate);\n\n    if (!templateResult.success) {\n      debug('[Permissions] Invalid template format:', templateResult.error.issues);\n      return;\n    }\n\n    const templateConfig: PermissionsConfigFile = templateResult.data;\n\n    // Merge user customizations with new template\n    const mergedConfig = mergePermissionsConfigs(templateConfig, userConfig);\n\n    // Write the merged config\n    writeFileSync(destPath, JSON.stringify(mergedConfig, null, 2), 'utf-8');\n    debug('[Permissions] Updated default permissions to language:', language);\n  } catch (error) {\n    debug('[Permissions] Error updating permissions language:', error);\n  }\n}\n\n/**\n * Check if a comment is user-modified (starts with [USER] prefix)\n */\nfunction isUserComment(comment: string | undefined): boolean {\n  return comment !== undefined && comment.trim().startsWith('[USER]');\n}\n\n/**\n * Merge user customizations into a new template.\n * Preserves user-added patterns while updating system default comments.\n * Protects user-modified comments from being overwritten.\n */\nfunction mergePermissionsConfigs(\n  template: PermissionsConfigFile,\n  userConfig: PermissionsConfigFile | null\n): PermissionsConfigFile {\n  if (!userConfig) {\n    return template;\n  }\n\n  // Start with the template as base\n  const merged: PermissionsConfigFile = JSON.parse(JSON.stringify(template));\n\n  // Helper to check if a pattern exists in template\n  const patternExistsInTemplate = (\n    pattern: string,\n    templateArray: Array<string | { pattern: string; comment?: string }>\n  ): boolean => {\n    return templateArray.some(item => {\n      const itemPattern = typeof item === 'string' ? item : item.pattern;\n      return itemPattern === pattern;\n    });\n  };\n\n  // Helper to merge patterns with user comment protection\n  const mergePatterns = (\n    templatePatterns: Array<string | { pattern: string; comment?: string; userModified?: boolean }> | undefined,\n    userPatterns: Array<string | { pattern: string; comment?: string; userModified?: boolean }> | undefined\n  ): Array<string | { pattern: string; comment?: string; userModified?: boolean }> => {\n    const result = [...(templatePatterns || [])];\n\n    if (!userPatterns) return result;\n\n    // Process each user pattern\n    userPatterns.forEach(userItem => {\n      const userPattern = typeof userItem === 'string' ? userItem : userItem.pattern;\n\n      // Find if this pattern exists in template\n      const templateIndex = result.findIndex(item => {\n        const itemPattern = typeof item === 'string' ? item : item.pattern;\n        return itemPattern === userPattern;\n      });\n\n      if (templateIndex === -1) {\n        // Pattern not in template - user added it, preserve completely\n        result.push(userItem);\n      } else if (typeof userItem !== 'string') {\n        // Pattern exists in template - check if user modified the comment\n        const isModified = userItem.userModified || isUserComment(userItem.comment);\n\n        if (isModified) {\n          // User modified this rule - preserve the user's comment\n          result[templateIndex] = {\n            pattern: userPattern,\n            comment: userItem.comment,\n            userModified: true,\n          };\n        }\n        // Otherwise, use template's comment (language update scenario)\n      }\n    });\n\n    return result;\n  };\n\n  // Merge bash patterns with user comment protection\n  if (userConfig.allowedBashPatterns || template.allowedBashPatterns) {\n    merged.allowedBashPatterns = mergePatterns(\n      template.allowedBashPatterns,\n      userConfig.allowedBashPatterns\n    );\n  }\n\n  // Merge MCP patterns with user comment protection\n  if (userConfig.allowedMcpPatterns || template.allowedMcpPatterns) {\n    merged.allowedMcpPatterns = mergePatterns(\n      template.allowedMcpPatterns,\n      userConfig.allowedMcpPatterns\n    );\n  }\n\n  // Merge write paths with user comment protection\n  if (userConfig.allowedWritePaths || template.allowedWritePaths) {\n    merged.allowedWritePaths = mergePatterns(\n      template.allowedWritePaths,\n      userConfig.allowedWritePaths\n    );\n  }\n\n  // Merge API endpoints - preserve user additions\n  if (userConfig.allowedApiEndpoints && userConfig.allowedApiEndpoints.length > 0) {\n    merged.allowedApiEndpoints = [\n      ...(template.allowedApiEndpoints || []),\n      ...userConfig.allowedApiEndpoints\n    ];\n  }\n\n  return merged;\n}\n\n/**\n * Load default permissions from ~/.craft-agent/permissions/default.json\n * Returns null if file doesn't exist or is invalid.\n */\nexport function loadDefaultPermissions(): PermissionsCustomConfig | null {\n  const defaultPath = join(getAppPermissionsDir(), 'default.json');\n  if (!existsSync(defaultPath)) {\n    debug('[Permissions] No default.json found at', defaultPath);\n    return null;\n  }\n\n  try {\n    const content = readFileSync(defaultPath, 'utf-8');\n    const config = parsePermissionsJson(content);\n    debug('[Permissions] Loaded default permissions from', defaultPath);\n    return config;\n  } catch (error) {\n    debug('[Permissions] Error loading default permissions:', error);\n    return null;\n  }\n}\n\n// Re-export types from mode-types for external consumers\nexport {\n  PermissionsConfigSchema,\n  type ApiEndpointRule,\n  type PermissionsConfigFile,\n  type CompiledApiEndpointRule,\n  type CompiledBashPattern,\n  type PermissionPaths,\n};\n\n// ============================================================\n// Types\n// ============================================================\n\n/**\n * Pattern entry with optional comment for error messages.\n * Preserves the comment from permissions.json so we can show helpful hints.\n */\nexport interface PatternWithComment {\n  pattern: string;\n  comment?: string;\n  userModified?: boolean;\n}\n\n/**\n * Parsed and normalized permissions configuration\n *\n * Note: blockedTools (Write, Edit, MultiEdit, NotebookEdit) are hardcoded in\n * SAFE_MODE_CONFIG and not configurable here - they're fundamental write\n * operations that must always be blocked in Explore mode.\n */\nexport interface PermissionsCustomConfig {\n  /** Additional bash patterns to allow (with optional comments for error messages) */\n  allowedBashPatterns: PatternWithComment[];\n  /** Additional MCP patterns to allow (as regex strings) */\n  allowedMcpPatterns: string[];\n  /** API endpoint rules for fine-grained control */\n  allowedApiEndpoints: ApiEndpointRule[];\n  /** File paths to allow writes in Explore mode (glob pattern strings) */\n  allowedWritePaths: string[];\n}\n\n/**\n * Merged permissions config for runtime use\n */\nexport interface MergedPermissionsConfig {\n  /** Blocked tools (Write, Edit, MultiEdit, NotebookEdit) - hardcoded, not configurable */\n  blockedTools: Set<string>;\n  /** Read-only bash patterns with metadata for helpful error messages */\n  readOnlyBashPatterns: CompiledBashPattern[];\n  readOnlyMcpPatterns: RegExp[];\n  /** Fine-grained API endpoint rules */\n  allowedApiEndpoints: CompiledApiEndpointRule[];\n  /** File paths allowed for writes in Explore mode (glob patterns) */\n  allowedWritePaths: string[];\n  /** Display name for error messages */\n  displayName: string;\n  /** Keyboard shortcut hint */\n  shortcutHint: string;\n  /** Paths to permission files for actionable error messages */\n  permissionPaths?: PermissionPaths;\n}\n\n/**\n * Context for permissions checking (includes workspace/source/agent info)\n */\nexport interface PermissionsContext {\n  workspaceRootPath: string;\n  /** Active source slugs for source-specific rules */\n  activeSourceSlugs?: string[];\n}\n\n// ============================================================\n// JSON Parser\n// ============================================================\n\n/**\n * Parse and validate permissions.json file\n */\nexport function parsePermissionsJson(content: string): PermissionsCustomConfig {\n  const emptyConfig: PermissionsCustomConfig = {\n    allowedBashPatterns: [],\n    allowedMcpPatterns: [],\n    allowedApiEndpoints: [],\n    allowedWritePaths: [],\n  };\n\n  try {\n    const json = JSON.parse(content);\n    const result = PermissionsConfigSchema.safeParse(json);\n\n    if (!result.success) {\n      debug('[SafeMode] Validation errors:', result.error.issues);\n      // Log specific errors for debugging\n      for (const issue of result.error.issues) {\n        debug(`[SafeMode]   - ${issue.path.join('.')}: ${issue.message}`);\n      }\n      return emptyConfig;\n    }\n\n    const data = result.data;\n\n    // Normalize patterns (extract string from pattern objects, but NOT for bash - preserve comments)\n    const normalizePatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): string[] => {\n      if (!patterns) return [];\n      return patterns.map(p => typeof p === 'string' ? p : p.pattern);\n    };\n\n    // For bash patterns, preserve comments for helpful error messages\n    const normalizeBashPatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): PatternWithComment[] => {\n      if (!patterns) return [];\n      return patterns.map(p => {\n        if (typeof p === 'string') {\n          return { pattern: p };\n        }\n        return { pattern: p.pattern, comment: p.comment };\n      });\n    };\n\n    return {\n      allowedBashPatterns: normalizeBashPatterns(data.allowedBashPatterns),\n      allowedMcpPatterns: normalizePatterns(data.allowedMcpPatterns),\n      allowedApiEndpoints: data.allowedApiEndpoints ?? [],\n      allowedWritePaths: normalizePatterns(data.allowedWritePaths),\n    };\n  } catch (error) {\n    debug('[SafeMode] JSON parse error:', error);\n    return emptyConfig;\n  }\n}\n\n/**\n * Validate a regex pattern string, return null if invalid\n */\nfunction validateRegex(pattern: string): RegExp | null {\n  try {\n    return new RegExp(pattern);\n  } catch {\n    return null;\n  }\n}\n\n/**\n * Validate permissions config and