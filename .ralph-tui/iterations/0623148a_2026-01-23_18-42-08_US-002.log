E_CONFIG and not loaded from JSON.\n   668â†’   */\n   669â†’  private applyDefaultConfig(merged: MergedPermissionsConfig, config: PermissionsCustomConfig): void {\n   670â†’    // Add allowed bash patterns (as CompiledBashPattern with metadata for error messages)\n   671â†’    for (const patternEntry of config.allowedBashPatterns) {\n   672â†’      const regex = validateRegex(patternEntry.pattern);\n   673â†’      if (regex) {\n   674â†’        merged.readOnlyBashPatterns.push({\n   675â†’          regex,\n   676â†’          source: patternEntry.pattern,\n   677â†’          comment: patternEntry.comment,\n   678â†’        });\n   679â†’      } else {\n   680â†’        debug(`[Permissions] Invalid default bash pattern, skipping: ${patternEntry.pattern}`);\n   681â†’      }\n   682â†’    }\n   683â†’\n   684â†’    // Add allowed MCP patterns\n   685â†’    for (const pattern of config.allowedMcpPatterns) {\n   686â†’      const regex = validateRegex(pattern);\n   687â†’      if (regex) {\n   688â†’        merged.readOnlyMcpPatterns.push(regex);\n   689â†’      } else {\n   690â†’        debug(`[Permissions] Invalid default MCP pattern, skipping: ${pattern}`);\n   691â†’      }\n   692â†’    }\n   693â†’\n   694â†’    // Add allowed API endpoints\n   695â†’    for (const rule of config.allowedApiEndpoints) {\n   696â†’      const pathRegex = validateRegex(rule.path);\n   697â†’      if (pathRegex) {\n   698â†’        merged.allowedApiEndpoints.push({\n   699â†’          method: rule.method,\n   700â†’          pathPattern: pathRegex,\n   701â†’        });\n   702â†’      }\n   703â†’    }\n   704â†’\n   705â†’    // Add allowed write paths\n   706â†’    for (const pattern of config.allowedWritePaths) {\n   707â†’      merged.allowedWritePaths.push(pattern);\n   708â†’    }\n   709â†’  }\n   710â†’\n   711â†’  private applyCustomConfig(merged: MergedPermissionsConfig, custom: PermissionsCustomConfig): void {\n   712â†’    // Add allowed bash patterns (making config more