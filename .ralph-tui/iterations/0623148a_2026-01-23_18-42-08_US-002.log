{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_vrtx_01FzJe1arbvntAeZmPCyMCJR","type":"tool_result","content":"     1â†’/**\n     2â†’ * Safe Mode Configuration\n     3â†’ *\n     4â†’ * Allows customization of Safe Mode rules per workspace and per source.\n     5â†’ * Users can create permissions.json files to extend the default rules.\n     6â†’ *\n     7â†’ * File locations:\n     8â†’ * - Workspace: ~/.craft-agent/workspaces/{slug}/permissions.json\n     9â†’ * - Per-source: ~/.craft-agent/workspaces/{slug}/sources/{sourceSlug}/permissions.json\n    10â†’ *\n    11â†’ * Rules are additive - custom configs extend the defaults (more permissive).\n    12â†’ */\n    13â†’\n    14â†’import { existsSync, readFileSync, mkdirSync, writeFileSync } from 'fs';\n    15â†’import { join } from 'path';\n    16â†’import { debug } from '../utils/debug.ts';\n    17â†’import { CONFIG_DIR } from '../config/paths.ts';\n    18â†’import { getSourcePath } from '../sources/storage.ts';\n    19â†’import {\n    20â†’  SAFE_MODE_CONFIG,\n    21â†’  PermissionsConfigSchema,\n    22â†’  type ApiEndpointRule,\n    23â†’  type PermissionsConfigFile,\n    24â†’  type CompiledApiEndpointRule,\n    25â†’  type CompiledBashPattern,\n    26â†’  type PermissionPaths,\n    27â†’} from './mode-types.ts';\n    28â†’\n    29â†’// ============================================================\n    30â†’// App-level Permissions Directory\n    31â†’// ============================================================\n    32â†’\n    33â†’const APP_PERMISSIONS_DIR = join(CONFIG_DIR, 'permissions');\n    34â†’\n    35â†’/**\n    36â†’ * Get the app-level permissions directory.\n    37â†’ * Default permissions are stored at ~/.craft-agent/permissions/\n    38â†’ */\n    39â†’export function getAppPermissionsDir(): string {\n    40â†’  return APP_PERMISSIONS_DIR;\n    41â†’}\n    42â†’\n    43â†’/**\n    44â†’ * Ensure default permissions file exists.\n    45â†’ * Copies bundled default.json from the provided directory on first run.\n    46â†’ * Only copies if file doesn't exist (preserves user edits).\n    47â†’ * @param bundledPermissionsDir - Path to bundled permissions (e.g., Electron's resources/permissions)\n    48â†’ * @param language - Language code (e.g., 'en', 'zh-CN') for selecting the appropriate template\n    49â†’ */\n    50â†’export function ensureDefaultPermissions(bundledPermissionsDir?: string, language?: string): void {\n    51â†’  const permissionsDir = getAppPermissionsDir();\n    52â†’\n    53â†’  // Create permissions directory if it doesn't exist\n    54â†’  if (!existsSync(permissionsDir)) {\n    55â†’    mkdirSync(permissionsDir, { recursive: true });\n    56â†’  }\n    57â†’\n    58â†’  // If no bundled permissions directory provided, just ensure the directory exists\n    59â†’  if (!bundledPermissionsDir || !existsSync(bundledPermissionsDir)) {\n    60â†’    return;\n    61â†’  }\n    62â†’\n    63â†’  // Copy default.json if it doesn't exist in app permissions dir\n    64â†’  const destPath = join(permissionsDir, 'default.json');\n    65â†’  if (!existsSync(destPath)) {\n    66â†’    const templateFile = language ? `default.${language}.json` : 'default.en.json';\n    67â†’    const srcPath = join(bundledPermissionsDir, templateFile);\n    68â†’\n    69â†’    // Fallback to English if the language-specific template doesn't exist\n    70â†’    const finalSrcPath = existsSync(srcPath)\n    71â†’      ? srcPath\n    72â†’      : join(bundledPermissionsDir, 'default.en.json');\n    73â†’\n    74â†’    if (existsSync(finalSrcPath)) {\n    75â†’      try {\n    76â†’        const content = readFileSync(finalSrcPath, 'utf-8');\n    77â†’        writeFileSync(destPath, content, 'utf-8');\n    78â†’        debug('[Permissions] Copied bundled permissions template to', destPath);\n    79â†’      } catch (error) {\n    80â†’        debug('[Permissions] Error copying bundled permissions template:', error);\n    81â†’      }\n    82â†’    }\n    83â†’  }\n    84â†’}\n    85â†’\n    86â†’/**\n    87â†’ * Update default permissions file to match the current language.\n    88â†’ * Preserves user customizations while updating system default comments.\n    89â†’ * @param bundledPermissionsDir - Path to bundled permissions templates\n    90â†’ * @param language - Target language code (e.g., 'en', 'zh-CN')\n    91â†’ */\n    92â†’export function updateDefaultPermissionsLanguage(bundledPermissionsDir: string, language: string): void {\n    93â†’  const permissionsDir = getAppPermissionsDir();\n    94â†’  const destPath = join(permissionsDir, 'default.json');\n    95â†’\n    96â†’  if (!existsSync(bundledPermissionsDir)) {\n    97â†’    debug('[Permissions] Bundled permissions directory not found:', bundledPermissionsDir);\n    98â†’    return;\n    99â†’  }\n   100â†’\n   101â†’  // Load the new language template\n   102â†’  const templateFile = `default.${language}.json`;\n   103â†’  const templatePath = join(bundledPermissionsDir, templateFile);\n   104â†’\n   105â†’  // Fallback to English if the language-specific template doesn't exist\n   106â†’  const finalTemplatePath = existsSync(templatePath)\n   107â†’    ? templatePath\n   108â†’    : join(bundledPermissionsDir, 'default.en.json');\n   109â†’\n   110â†’  if (!existsSync(finalTemplatePath)) {\n   111â†’    debug('[Permissions] Template not found:', finalTemplatePath);\n   112â†’    return;\n   113â†’  }\n   114â†’\n   115â†’  try {\n   116â†’    // Load existing user config (if any)\n   117â†’    let userConfig: PermissionsConfigFile | null = null;\n   118â†’    if (existsSync(destPath)) {\n   119â†’      const existingContent = readFileSync(destPath, 'utf-8');\n   120â†’      userConfig = parsePermissionsJson(existingContent);\n   121â†’    }\n   122â†’\n   123â†’    // Load the new template\n   124â†’    const templateContent = readFileSync(finalTemplatePath, 'utf-8');\n   125â†’    const parsedTemplate = JSON.parse(templateContent);\n   126â†’    const templateResult = PermissionsConfigSchema.safeParse(parsedTemplate);\n   127â†’\n   128â†’    if (!templateResult.success) {\n   129â†’      debug('[Permissions] Invalid template format:', templateResult.error.issues);\n   130â†’      return;\n   131â†’    }\n   132â†’\n   133â†’    const templateConfig: PermissionsConfigFile = templateResult.data;\n   134â†’\n   135â†’    // Merge user customizations with new template\n   136â†’    const mergedConfig = mergePermissionsConfigs(templateConfig, userConfig);\n   137â†’\n   138â†’    // Write the merged config\n   139â†’    writeFileSync(destPath, JSON.stringify(mergedConfig, null, 2), 'utf-8');\n   140â†’    debug('[Permissions] Updated default permissions to language:', language);\n   141â†’  } catch (error) {\n   142â†’    debug('[Permissions] Error updating permissions language:', error);\n   143â†’  }\n   144â†’}\n   145â†’\n   146â†’/**\n   147â†’ * Merge user customizations into a new template.\n   148â†’ * Preserves user-added patterns while updating system default comments.\n   149â†’ */\n   150â†’function mergePermissionsConfigs(\n   151â†’  template: PermissionsConfigFile,\n   152â†’  userConfig: PermissionsConfigFile | null\n   153â†’): PermissionsConfigFile {\n   154â†’  if (!userConfig) {\n   155â†’    return template;\n   156â†’  }\n   157â†’\n   158â†’  // Start with the template as base\n   159â†’  const merged: PermissionsConfigFile = JSON.parse(JSON.stringify(template));\n   160â†’\n   161â†’  // Helper to check if a pattern exists in template\n   162â†’  const patternExistsInTemplate = (\n   163â†’    pattern: string,\n   164â†’    templateArray: Array<string | { pattern: string; comment?: string }>\n   165â†’  ): boolean => {\n   166â†’    return templateArray.some(item => {\n   167â†’      const itemPattern = typeof item === 'string' ? item : item.pattern;\n   168â†’      return itemPattern === pattern;\n   169â†’    });\n   170â†’  };\n   171â†’\n   172â†’  // Merge bash patterns - add user custom patterns not in template\n   173â†’  if (userConfig.allowedBashPatterns) {\n   174â†’    const templateBashPatterns = template.allowedBashPatterns || [];\n   175â†’    userConfig.allowedBashPatterns.forEach(userItem => {\n   176â†’      const userPattern = typeof userItem === 'string' ? userItem : userItem.pattern;\n   177â†’      if (!patternExistsInTemplate(userPattern, templateBashPatterns)) {\n   178â†’        // User added this custom pattern - preserve it\n   179â†’        merged.allowedBashPatterns = merged.allowedBashPatterns || [];\n   180â†’        merged.allowedBashPatterns.push(userItem);\n   181â†’      }\n   182â†’    });\n   183â†’  }\n   184â†’\n   185â†’  // Merge MCP patterns - add user custom patterns not in template\n   186â†’  if (userConfig.allowedMcpPatterns) {\n   187â†’    const templateMcpPatterns = template.allowedMcpPatterns || [];\n   188â†’    userConfig.allowedMcpPatterns.forEach(userItem => {\n   189â†’      const userPattern = typeof userItem === 'string' ? userItem : userItem.pattern;\n   190â†’      if (!patternExistsInTemplate(userPattern, templateMcpPatterns)) {\n   191â†’        merged.allowedMcpPatterns = merged.allowedMcpPatterns || [];\n   192â†’        merged.allowedMcpPatterns.push(userItem);\n   193â†’      }\n   194â†’    });\n   195â†’  }\n   196â†’\n   197â†’  // Merge API endpoints - preserve user additions\n   198â†’  if (userConfig.allowedApiEndpoints && userConfig.allowedApiEndpoints.length > 0) {\n   199â†’    merged.allowedApiEndpoints = [\n   200â†’      ...(template.allowedApiEndpoints || []),\n   201â†’      ...userConfig.allowedApiEndpoints\n   202â†’    ];\n   203â†’  }\n   204â†’\n   205â†’  // Merge write paths - preserve user additions\n   206â†’  if (userConfig.allowedWritePaths && userConfig.allowedWritePaths.length > 0) {\n   207â†’    const templateWritePaths = template.allowedWritePaths || [];\n   208â†’    userConfig.allowedWritePaths.forEach(userItem => {\n   209â†’      const userPath = typeof userItem === 'string' ? userItem : userItem.pattern;\n   210â†’      if (!patternExistsInTemplate(userPath, templateWritePaths)) {\n   211â†’        merged.allowedWritePaths = merged.allowedWritePaths || [];\n   212â†’        merged.allowedWritePaths.push(userItem);\n   213â†’      }\n   214â†’    });\n   215â†’  }\n   216â†’\n   217â†’  return merged;\n   218â†’}\n   219â†’\n   220â†’/**\n   221â†’ * Load default permissions from ~/.craft-agent/permissions/default.json\n   222â†’ * Returns null if file doesn't exist or is invalid.\n   223â†’ */\n   224â†’export function loadDefaultPermissions(): PermissionsCustomConfig | null {\n   225â†’  const defaultPath = join(getAppPermissionsDir(), 'default.json');\n   226â†’  if (!existsSync(defaultPath)) {\n   227â†’    debug('[Permissions] No default.json found at', defaultPath);\n   228â†’    return null;\n   229â†’  }\n   230â†’\n   231â†’  try {\n   232â†’    const content = readFileSync(defaultPath, 'utf-8');\n   233â†’    const config = parsePermissionsJson(content);\n   234â†’    debug('[Permissions] Loaded default permissions from', defaultPath);\n   235â†’    return config;\n   236â†’  } catch (error) {\n   237â†’    debug('[Permissions] Error loading default permissions:', error);\n   238â†’    return null;\n   239â†’  }\n   240â†’}\n   241â†’\n   242â†’// Re-export types from mode-types for external consumers\n   243â†’export {\n   244â†’  PermissionsConfigSchema,\n   245â†’  type ApiEndpointRule,\n   246â†’  type PermissionsConfigFile,\n   247â†’  type CompiledApiEndpointRule,\n   248â†’  type CompiledBashPattern,\n   249â†’  type PermissionPaths,\n   250â†’};\n   251â†’\n   252â†’// ============================================================\n   253â†’// Types\n   254â†’// ============================================================\n   255â†’\n   256â†’/**\n   257â†’ * Pattern entry with optional comment for error messages.\n   258â†’ * Preserves the comment from permissions.json so we can show helpful hints.\n   259â†’ */\n   260â†’export interface PatternWithComment {\n   261â†’  pattern: string;\n   262â†’  comment?: string;\n   263â†’}\n   264â†’\n   265â†’/**\n   266â†’ * Parsed and normalized permissions configuration\n   267â†’ *\n   268â†’ * Note: blockedTools (Write, Edit, MultiEdit, NotebookEdit) are hardcoded in\n   269â†’ * SAFE_MODE_CONFIG and not configurable here - they're fundamental write\n   270â†’ * operations that must always be blocked in Explore mode.\n   271â†’ */\n   272â†’export interface PermissionsCustomConfig {\n   273â†’  /** Additional bash patterns to allow (with optional comments for error messages) */\n   274â†’  allowedBashPatterns: PatternWithComment[];\n   275â†’  /** Additional MCP patterns to allow (as regex strings) */\n   276â†’  allowedMcpPatterns: string[];\n   277â†’  /** API endpoint rules for fine-grained control */\n   278â†’  allowedApiEndpoints: ApiEndpointRule[];\n   279â†’  /** File paths to allow writes in Explore mode (glob pattern strings) */\n   280â†’  allowedWritePaths: string[];\n   281â†’}\n   282â†’\n   283â†’/**\n   284â†’ * Merged permissions config for runtime use\n   285â†’ */\n   286â†’export interface MergedPermissionsConfig {\n   287â†’  /** Blocked tools (Write, Edit, MultiEdit, NotebookEdit) - hardcoded, not configurable */\n   288â†’  blockedTools: Set<string>;\n   289â†’  /** Read-only bash patterns with metadata for helpful error messages */\n   290â†’  readOnlyBashPatterns: CompiledBashPattern[];\n   291â†’  readOnlyMcpPatterns: RegExp[];\n   292â†’  /** Fine-grained API endpoint rules */\n   293â†’  allowedApiEndpoints: CompiledApiEndpointRule[];\n   294â†’  /** File paths allowed for writes in Explore mode (glob patterns) */\n   295â†’  allowedWritePaths: string[];\n   296â†’  /** Display name for error messages */\n   297â†’  displayName: string;\n   298â†’  /** Keyboard shortcut hint */\n   299â†’  shortcutHint: string;\n   300â†’  /** Paths to permission files for actionable error messages */\n   301â†’  permissionPaths?: PermissionPaths;\n   302â†’}\n   303â†’\n   304â†’/**\n   305â†’ * Context for permissions checking (includes workspace/source/agent info)\n   306â†’ */\n   307â†’export interface PermissionsContext {\n   308â†’  workspaceRootPath: string;\n   309â†’  /** Active source slugs for source-specific rules */\n   310â†’  activeSourceSlugs?: string[];\n   311â†’}\n   312â†’\n   313â†’// ============================================================\n   314â†’// JSON Parser\n   315â†’// ============================================================\n   316â†’\n   317â†’/**\n   318â†’ * Parse and validate permissions.json file\n   319â†’ */\n   320â†’export function parsePermissionsJson(content: string): PermissionsCustomConfig {\n   321â†’  const emptyConfig: PermissionsCustomConfig = {\n   322â†’    allowedBashPatterns: [],\n   323â†’    allowedMcpPatterns: [],\n   324â†’    allowedApiEndpoints: [],\n   325â†’    allowedWritePaths: [],\n   326â†’  };\n   327â†’\n   328â†’  try {\n   329â†’    const json = JSON.parse(content);\n   330â†’    const result = PermissionsConfigSchema.safeParse(json);\n   331â†’\n   332â†’    if (!result.success) {\n   333â†’      debug('[SafeMode] Validation errors:', result.error.issues);\n   334â†’      // Log specific errors for debugging\n   335â†’      for (const issue of result.error.issues) {\n   336â†’        debug(`[SafeMode]   - ${issue.path.join('.')}: ${issue.message}`);\n   337â†’      }\n   338â†’      return emptyConfig;\n   339â†’    }\n   340â†’\n   341â†’    const data = result.data;\n   342â†’\n   343â†’    // Normalize patterns (extract string from pattern objects, but NOT for bash - preserve comments)\n   344â†’    const normalizePatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): string[] => {\n   345â†’      if (!patterns) return [];\n   346â†’      return patterns.map(p => typeof p === 'string' ? p : p.pattern);\n   347â†’    };\n   348â†’\n   349â†’    // For bash patterns, preserve comments for helpful error messages\n   350â†’    const normalizeBashPatterns = (patterns: Array<string | { pattern: string; comment?: string }> | undefined): PatternWithComment[] => {\n   351â†’      if (!patterns) return [];\n   352â†’      return patterns.map(p => {\n   353â†’        if (typeof p === 'string') {\n   354â†’          return { pattern: p };\n   355â†’        }\n   356â†’        return { pattern: p.pattern, comment: p.comment };\n   357â†’      });\n   358â†’    };\n   359â†’\n   360â†’    return {\n   361â†’      allowedBashPatterns: normalizeBashPatterns(data.allowedBashPatterns),\n   362â†’      allowedMcpPatterns: normalizePatterns(data.allowedMcpPatterns),\n   363â†’      allowedApiEndpoints: data.allowedApiEndpoints ?? [],\n   364â†’      allowedWritePaths: normalizePatterns(data.allowedWritePaths),\n   365â†’    };\n   366â†’  } catch (error) {\n   367â†’    debug('[SafeMode] JSON parse error:', error);\n   368â†’    return emptyConfig;\n   369â†’  }\n   370â†’}\n   371â†’\n   372â†’/**\n   373â†’ * Validate a regex pattern string, return null if invalid\n   374â†’ */\n   375â†’function validateRegex(pattern: string): RegExp | null {\n   376â†’  try {\n   377â†’    return new RegExp(pattern);\n   378â†’  } catch {\n   379â†’    return null;\n   380â†’  }\n   381â†’}\n   382â†’\n   383â†’/**\n   384â†’ * Validate permissions config and return errors\n   385â†’ */\n   386â†’export function validatePermissionsConfig(config: PermissionsConfigFile): string[] {\n   387â†’  const errors: string[] = [];\n   388â†’\n   389â†’  // Validate regex patterns\n   390â†’  const checkPatterns = (patterns: Array<string | { pattern: string }> | undefined, name: string) => {\n   391â†’    if (!patterns) return;\n   392â†’    for (let i = 0; i < patterns.length; i++) {\n   393â†’      const p = patterns[i];\n   394â†’      if (!p) continue;\n   395â†’      const patternStr = typeof p === 'string' ? p : p.pattern;\n   396â†’      if (!validateRegex(patternStr)) {\n   397â†’        errors.push(`${name}[${i}]: Invalid regex pattern: ${patternStr}`);\n   398â†’      }\n   399â†’    }\n   400â†’  };\n   401â†’\n   402â†’  checkPatterns(config.allowedBashPatterns, 'allowedBashPatterns');\n   403â†’  checkPatterns(config.allowedMcpPatterns, 'allowedMcpPatterns');\n   404â†’\n   405â†’  // Validate API endpoint patterns\n   406â†’  if (config.allowedApiEndpoints) {\n   407â†’    for (let i = 0; i < config.allowedApiEndpoints.length; i++) {\n   408â†’      const rule = config.allowedApiEndpoints[i];\n   409â†’      if (rule && !validateRegex(rule.path)) {\n   410â†’        errors.push(`allowedApiEndpoints[${i}].path: Invalid regex pattern: ${rule.path}`);\n   411â†’      }\n   412â†’    }\n   413â†’  }\n   414â†’\n   415â†’  return errors;\n   416â†’}\n   417â†’\n   418â†’// ============================================================\n   419â†’// Storage Functions\n   420â†’// ============================================================\n   421â†’\n   422â†’/**\n   423â†’ * Get path to workspace permissions.json\n   424â†’ */\n   425â†’export function getWorkspacePermissionsPath(workspaceRootPath: string): string {\n   426â†’  return join(workspaceRootPath, 'permissions.json');\n   427â†’}\n   428â†’\n   429â†’/**\n   430â†’ * Get path to source permissions.json\n   431â†’ */\n   432â†’export function getSourcePermissionsPath(workspaceRootPath: string, sourceSlug: string): string {\n   433â†’  return join(getSourcePath(workspaceRootPath, sourceSlug), 'permissions.json');\n   434â†’}\n   435â†’\n   436â†’/**\n   437â†’ * Load workspace-level permissions config\n   438â†’ */\n   439â†’export function loadWorkspacePermissionsConfig(workspaceRootPath: string): PermissionsCustomConfig | null {\n   440â†’  const path = getWorkspacePermissionsPath(workspaceRootPath);\n   441â†’  if (!existsSync(path)) return null;\n   442â†’\n   443â†’  try {\n   444â†’    const content = readFileSync(path, 'utf-8');\n   445â†’    const config = parsePermissionsJson(content);\n   446â†’    debug(`[Permissions] Loaded workspace config from ${path}:`, config);\n   447â†’    return config;\n   448â†’  } catch (error) {\n   449â†’    debug(`[Permissions] Error loading workspace config:`, error);\n   450â†’    return null;\n   451â†’  }\n   452â†’}\n   453â†’\n   454â†’/**\n   455â†’ * Load source-level permissions config\n   456â†’ */\n   457â†’export function loadSourcePermissionsConfig(\n   458â†’  workspaceRootPath: string,\n   459â†’  sourceSlug: string\n   460â†’): PermissionsCustomConfig | null {\n   461â†’  const path = getSourcePermissionsPath(workspaceRootPath, sourceSlug);\n   462â†’  if (!existsSync(path)) return null;\n   463â†’\n   464â†’  try {\n   465â†’    const content = readFileSync(path, 'utf-8');\n   466â†’    const config = parsePermissionsJson(content);\n   467â†’    debug(`[Permissions] Loaded source config from ${path}:`, config);\n   468â†’    return config;\n   469â†’  } catch (error) {\n   470â†’    debug(`[Permissions] Error loading source config:`, error);\n   471â†’    return null;\n   472â†’  }\n   473â†’}\n   474â†’\n   475â†’// ============================================================\n   476â†’// API Endpoint Checking\n   477â†’// ============================================================\n   478â†’\n   479â†’/**\n   480â†’ * Check if an API call is allowed by endpoint rules\n   481â†’ */\n   482â†’export function isApiEndpointAllowed(\n   483â†’  method: string,\n   484â†’  path: string,\n   485â†’  config: MergedPermissionsConfig\n   486â†’): boolean {\n   487â†’  const upperMethod = method.toUpperCase();\n   488â†’\n   489â†’  // GET is always allowed\n   490â†’  if (upperMethod === 'GET') return true;\n   491â†’\n   492â†’  // Check fine-grained endpoint rules\n   493â†’  for (const rule of config.allowedApiEndpoints) {\n   494â†’    if (rule.method === upperMethod && rule.pathPattern.test(path)) {\n   495â†’      return true;\n   496â†’    }\n   497â†’  }\n   498â†’\n   499â†’  return false;\n   500â†’}\n   501â†’\n   502â†’// ============================================================\n   503â†’// Config Cache\n   504â†’// ============================================================\n   505â†’\n   506â†’/**\n   507â†’ * In-memory cache for parsed permissions configs\n   508â†’ * Invalidated on file changes via ConfigWatcher\n   509â†’ */\n   510â†’class PermissionsConfigCache {\n   511â†’  private workspaceConfigs: Map<string, PermissionsCustomConfig | null> = new Map();\n   512â†’  private sourceConfigs: Map<string, PermissionsCustomConfig | null> = new Map();\n   513â†’  private mergedConfigs: Map<string, MergedPermissionsConfig> = new Map();\n   514â†’\n   515â†’  // App-level default permissions (loaded from ~/.craft-agent/permissions/default.json)\n   516â†’  private defaultConfig: PermissionsCustomConfig | null | undefined = undefined; // undefined = not loaded yet\n   517â†’\n   518â†’  /**\n   519â†’   * Get or load app-level default permissions\n   520â†’   * These come from ~/.craft-agent/permissions/default.json\n   521â†’   */\n   522â†’  private getDefaultConfig(): PermissionsCustomConfig | null {\n   523â†’    if (this.defaultConfig === undefined) {\n   524â†’      this.defaultConfig = loadDefaultPermissions();\n   525â†’    }\n   526â†’    return this.defaultConfig;\n   527â†’  }\n   528â†’\n   529â†’  /**\n   530â†’   * Get or load workspace config\n   531â†’   */\n   532â†’  getWorkspaceConfig(workspaceRootPath: string): PermissionsCustomConfig | null {\n   533â†’    if (!this.workspaceConfigs.has(workspaceRootPath)) {\n   534â†’      this.workspaceConfigs.set(workspaceRootPath, loadWorkspacePermissionsConfig(workspaceRootPath));\n   535â†’    }\n   536â†’    return this.workspaceConfigs.get(workspaceRootPath) ?? null;\n   537â†’  }\n   538â†’\n   539â†’  /**\n   540â†’   * Get or load source config\n   541â†’   */\n   542â†’  getSourceConfig(workspaceRootPath: string, sourceSlug: string): PermissionsCustomConfig | null {\n   543â†’    const key = `${workspaceRootPath}::${sourceSlug}`;\n   544â†’    if (!this.sourceConfigs.has(key)) {\n   545â†’      this.sourceConfigs.set(key, loadSourcePermissionsConfig(workspaceRootPath, sourceSlug));\n   546â†’    }\n   547â†’    return this.sourceConfigs.get(key) ?? null;\n   548â†’  }\n   549â†’\n   550â†’  /**\n   551â†’   * Invalidate app-level default permissions (called by ConfigWatcher)\n   552â†’   * This clears all merged configs since defaults affect everything\n   553â†’   */\n   554â†’  invalidateDefaults(): void {\n   555â†’    debug('[Permissions] Invalidating app-level default permissions');\n   556â†’    this.defaultConfig = undefined;\n   557â†’    // Clear ALL merged configs since defaults affect everything\n   558â†’    this.mergedConfigs.clear();\n   559â†’  }\n   560â†’\n   561â†’  /**\n   562â†’   * Invalidate workspace config (called by ConfigWatcher)\n   563â†’   */\n   564â†’  invalidateWorkspace(workspaceRootPath: string): void {\n   565â†’    debug(`[Permissions] Invalidating workspace config: ${workspaceRootPath}`);\n   566â†’    this.workspaceConfigs.delete(workspaceRootPath);\n   567â†’    // Clear all merged configs for this workspace\n   568â†’    for (const key of this.mergedConfigs.keys()) {\n   569â†’      if (key.startsWith(`${workspaceRootPath}::`)) {\n   570â†’        this.mergedConfigs.delete(key);\n   571â†’      }\n   572â†’    }\n   573â†’  }\n   574â†’\n   575â†’  /**\n   576â†’   * Invalidate source config (called by ConfigWatcher)\n   577â†’   */\n   578â†’  invalidateSource(workspaceRootPath: string, sourceSlug: string): void {\n   579â†’    debug(`[Permissions] Invalidating source config: ${workspaceRootPath}/${sourceSlug}`);\n   580â†’    this.sourceConfigs.delete(`${workspaceRootPath}::${sourceSlug}`);\n   581â†’    // Clear merged configs that include this source\n   582â†’    // Cache key format: \"{workspaceRootPath}::{source1},{source2},...\"\n   583â†’    // Use precise matching to avoid false positives (e.g., \"linear\" matching \"linear-triage\")\n   584â†’    for (const key of this.mergedConfigs.keys()) {\n   585â†’      if (!key.startsWith(`${workspaceRootPath}::`)) continue;\n   586â†’\n   587â†’      // Extract sources portion after the ::\n   588â†’      const sourcesStr = key.slice(workspaceRootPath.length + 2);\n   589â†’      if (!sourcesStr) continue;\n   590â†’\n   591â†’      // Check for exact match: at start, end, or between commas\n   592â†’      const sources = sourcesStr.split(',');\n   593â†’      if (sources.includes(sourceSlug)) {\n   594â†’        this.mergedConfigs.delete(key);\n   595â†’      }\n   596â†’    }\n   597â†’  }\n   598â†’\n   599â†’\n   600â†’  /**\n   601â†’   * Get merged config for a context (workspace + active sources)\n   602â†’   * Uses additive merging: custom configs extend defaults\n   603â†’   */\n   604â†’  getMergedConfig(context: PermissionsContext): MergedPermissionsConfig {\n   605â†’    const cacheKey = this.buildCacheKey(context);\n   606â†’\n   607â†’    if (!this.mergedConfigs.has(cacheKey)) {\n   608â†’      const merged = this.buildMergedConfig(context);\n   609â†’      this.mergedConfigs.set(cacheKey, merged);\n   610â†’    }\n   611â†’\n   612â†’    return this.mergedConfigs.get(cacheKey)!;\n   613â†’  }\n   614â†’\n   615â†’  private buildMergedConfig(context: PermissionsContext): MergedPermissionsConfig {\n   616â†’    const defaults = SAFE_MODE_CONFIG;\n   617â†’\n   618â†’    // Start with hardcoded fallback defaults (blocked tools are fixed, display settings)\n   619â†’    // blockedTools (Write, Edit, MultiEdit, NotebookEdit) come from SAFE_MODE_CONFIG\n   620â†’    // and cannot be modified via permissions.json\n   621â†’    const merged: MergedPermissionsConfig = {\n   622â†’      blockedTools: new Set(defaults.blockedTools),\n   623â†’      readOnlyBashPatterns: [...defaults.readOnlyBashPatterns],\n   624â†’      readOnlyMcpPatterns: [...defaults.readOnlyM